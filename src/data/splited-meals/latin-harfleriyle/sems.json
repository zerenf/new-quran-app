{
  "sems": {
    "surahNumber": "91",
    "surahNameTr": "Şems Suresi",
    "ayats": [
      {
        "ayahNumber": "1",
        "ayahText": "veşşemsi veḍuḥâhâ."
      },
      {
        "ayahNumber": "2",
        "ayahText": "velḳameri iẕâ telâhâ."
      },
      {
        "ayahNumber": "3",
        "ayahText": "vennehâri iẕâ cellâhâ."
      },
      {
        "ayahNumber": "4",
        "ayahText": "velleyli iẕâ yagşâhâ."
      },
      {
        "ayahNumber": "5",
        "ayahText": "vessemâi vemâ benâhâ."
      },
      {
        "ayahNumber": "6",
        "ayahText": "vel'arḍi vemâ ṭaḥâhâ."
      },
      {
        "ayahNumber": "7",
        "ayahText": "venefsiv vemâ sevvâhâ."
      },
      {
        "ayahNumber": "8",
        "ayahText": "feelhemehâ fücûrahâ vetaḳvâhâ."
      },
      {
        "ayahNumber": "9",
        "ayahText": "ḳad efleḥa men zekkâhâ."
      },
      {
        "ayahNumber": "10",
        "ayahText": "veḳad ḫâbe men dessâhâ."
      },
      {
        "ayahNumber": "11",
        "ayahText": "keẕẕebet ŝemûdü biṭagvâhâ."
      },
      {
        "ayahNumber": "12",
        "ayahText": "iẕi-mbe`aŝe eşḳâhâ."
      },
      {
        "ayahNumber": "13",
        "ayahText": "feḳâle lehüm rasûlü-llâhi nâḳate-llâhi vesuḳyâhâ."
      },
      {
        "ayahNumber": "14",
        "ayahText": "fekeẕẕebûhü fe`aḳarûhâ. fedemdeme `aleyhim rabbühüm biẕembihim fesevvâhâ."
      },
      {
        "ayahNumber": "15",
        "ayahText": "velâ yeḫâfü `uḳbâhâ."
      }
    ]
  }
}