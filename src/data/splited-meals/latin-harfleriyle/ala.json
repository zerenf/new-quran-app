{
  "ala": {
    "surahNumber": "87",
    "surahNameTr": "A'lâ Suresi",
    "ayats": [
      {
        "ayahNumber": "1",
        "ayahText": "sebbiḥi-sme rabbike-l'a`lâ."
      },
      {
        "ayahNumber": "2",
        "ayahText": "elleẕî ḫaleḳa fesevvâ."
      },
      {
        "ayahNumber": "3",
        "ayahText": "velleẕî ḳaddera fehedâ."
      },
      {
        "ayahNumber": "4",
        "ayahText": "velleẕî aḫrace-lmer`â."
      },
      {
        "ayahNumber": "5",
        "ayahText": "fece`alehû guŝâen aḥvâ."
      },
      {
        "ayahNumber": "6",
        "ayahText": "senuḳriüke felâ tensâ."
      },
      {
        "ayahNumber": "7",
        "ayahText": "illâ mâ şâe-llâh. innehû ya`lemü-lcehra vemâ yaḫfâ."
      },
      {
        "ayahNumber": "8",
        "ayahText": "venüyessiruke lilyüsrâ."
      },
      {
        "ayahNumber": "9",
        "ayahText": "feẕekkir in nefe`ati-ẕẕikrâ."
      },
      {
        "ayahNumber": "10",
        "ayahText": "seyeẕẕekkeru mey yaḫşâ."
      },
      {
        "ayahNumber": "11",
        "ayahText": "veyetecennebühe-l'eşḳâ."
      },
      {
        "ayahNumber": "12",
        "ayahText": "elleẕî yaṣle-nnâra-lkübrâ."
      },
      {
        "ayahNumber": "13",
        "ayahText": "ŝümme lâ yemûtü fîhâ velâ yaḥyâ."
      },
      {
        "ayahNumber": "14",
        "ayahText": "ḳad efleḥa men tezekkâ."
      },
      {
        "ayahNumber": "15",
        "ayahText": "veẕekera-sme rabbihî feṣallâ."
      },
      {
        "ayahNumber": "16",
        "ayahText": "bel tü'ŝirûne-lḥayâte-ddünyâ."
      },
      {
        "ayahNumber": "17",
        "ayahText": "vel'âḫiratü ḫayruv veebḳâ."
      },
      {
        "ayahNumber": "18",
        "ayahText": "inne hâẕâ lefi-ṣṣuḥufi-l'ûlâ."
      },
      {
        "ayahNumber": "19",
        "ayahText": "ṣuḥufi ibrâhîme vemûsâ."
      }
    ]
  }
}