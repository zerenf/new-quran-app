{
  "fecr": {
    "surahNumber": "89",
    "surahNameTr": "Fecr Suresi",
    "ayats": [
      {
        "ayahNumber": "1",
        "ayahText": "velfecr."
      },
      {
        "ayahNumber": "2",
        "ayahText": "veleyâlin `aşr."
      },
      {
        "ayahNumber": "3",
        "ayahText": "veşşef`i velvetr."
      },
      {
        "ayahNumber": "4",
        "ayahText": "velleyli iẕâ yesr."
      },
      {
        "ayahNumber": "5",
        "ayahText": "hel fî ẕâlike ḳasemül liẕî ḥicr."
      },
      {
        "ayahNumber": "6",
        "ayahText": "elem tera keyfe fe`ale rabbüke bi`âd."
      },
      {
        "ayahNumber": "7",
        "ayahText": "irame ẕâti-l`imâd."
      },
      {
        "ayahNumber": "8",
        "ayahText": "elletî lem yuḫlaḳ miŝlühâ fi-lbilâd."
      },
      {
        "ayahNumber": "9",
        "ayahText": "veŝemûde-lleẕîne câbu-ṣṣaḫra bilvâd."
      },
      {
        "ayahNumber": "10",
        "ayahText": "vefir`avne ẕi-l'evtâd."
      },
      {
        "ayahNumber": "11",
        "ayahText": "elleẕîne ṭagav fi-lbilâd."
      },
      {
        "ayahNumber": "12",
        "ayahText": "feekŝerû fîhe-lfesâd."
      },
      {
        "ayahNumber": "13",
        "ayahText": "feṣabbe `aleyhim rabbüke sevṭa `aẕâb."
      },
      {
        "ayahNumber": "14",
        "ayahText": "inne rabbeke lebilmirṣâd."
      },
      {
        "ayahNumber": "15",
        "ayahText": "feemme-l'insânü iẕâ me-btelâhü rabbühû feekramehû vene``amehû feyeḳûlü rabbî ekramen."
      },
      {
        "ayahNumber": "16",
        "ayahText": "veemmâ iẕâ me-btelâhü feḳadera `aleyhi rizḳahû feyeḳûlü rabbî ehânen."
      },
      {
        "ayahNumber": "17",
        "ayahText": "kellâ bel lâ tükrimûne-lyetîm."
      },
      {
        "ayahNumber": "18",
        "ayahText": "velâ teḥâḍḍûne `alâ ṭa`âmi-lmiskîn."
      },
      {
        "ayahNumber": "19",
        "ayahText": "vete'külûne-ttürâŝe eklel lemmâ."
      },
      {
        "ayahNumber": "20",
        "ayahText": "vetüḥibbûne-lmâle ḥubben cemmâ."
      },
      {
        "ayahNumber": "21",
        "ayahText": "kellâ iẕâ dükketi-l'arḍu dekken dekkâ."
      },
      {
        "ayahNumber": "22",
        "ayahText": "vecâe rabbüke velmelekü ṣaffen ṣaffâ."
      },
      {
        "ayahNumber": "23",
        "ayahText": "vecîe yevmeiẕim bicehenneme yevmeiẕiy yeteẕekkeru-l'insânü veennâ lehü-ẕẕikrâ."
      },
      {
        "ayahNumber": "24",
        "ayahText": "yeḳûlü yâ leytenî ḳaddemtü liḥayâtî."
      },
      {
        "ayahNumber": "25",
        "ayahText": "feyevmeiẕil lâ yü`aẕẕibü `aẕâbehû eḥad."
      },
      {
        "ayahNumber": "26",
        "ayahText": "velâ yûŝiḳu veŝâḳahû eḥad."
      },
      {
        "ayahNumber": "27",
        "ayahText": "yâ eyyetühe-nnefsü-lmuṭmeinneh."
      },
      {
        "ayahNumber": "28",
        "ayahText": "irci`î ilâ rabbiki râḍiyetem merḍiyyeh."
      },
      {
        "ayahNumber": "29",
        "ayahText": "fedḫulî fî `ibâdî."
      },
      {
        "ayahNumber": "30",
        "ayahText": "vedḫulî cennetî."
      }
    ]
  }
}