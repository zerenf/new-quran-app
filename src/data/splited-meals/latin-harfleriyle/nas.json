    "surahNameTr": "Nâs Suresi",
    "ayats": [
      {
        "ayahNumber": "1",
        "ayahText": "ḳul e`ûẕü birabbi-nnâs."
      },
      {
        "ayahNumber": "2",
        "ayahText": "meliki-nnâs."
      },
      {
        "ayahNumber": "3",
        "ayahText": "ilâhi-nnâs."
      },
      {
        "ayahNumber": "4",
        "ayahText": "min şerri-lvesvâsi-lḫannâs."
      },
      {
        "ayahNumber": "5",
        "ayahText": "elleẕî yüvesvisü fî ṣudûri-nnâs."
      },
      {
        "ayahNumber": "6",
        "ayahText": "mine-lcinneti vennâs."
      }
    ]
  }
