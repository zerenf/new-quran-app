{
  "tin": {
    "surahNumber": "95",
    "surahNameTr": "Tîn Suresi",
    "ayats": [
      {
        "ayahNumber": "1",
        "ayahText": "Tin'e ve Zeytun'a andolsun."
      },
      {
        "ayahNumber": "2",
        "ayahText": "Sina Dağına andolsun."
      },
      {
        "ayahNumber": "3",
        "ayahText": "Ve bu güvenli Şehre andolsun ki,"
      },
      {
        "ayahNumber": "4",
        "ayahText": "Biz insanı en güzel biçimde yarattık."
      },
      {
        "ayahNumber": "5",
        "ayahText": "Sonra onu aşağıların aşağısına çevirdik."
      },
      {
        "ayahNumber": "6",
        "ayahText": "Yalnız inanıp iyi işler yapanlar hariç. Onlar için kesintisiz bir mükafat vardır."
      },
      {
        "ayahNumber": "7",
        "ayahText": "Böyle iken sana ahiret cezasını yalanlatan nedir?"
      },
      {
        "ayahNumber": "8",
        "ayahText": "Allah, hüküm verenlerin en iyisi değil midir?"
      }
    ]
  }
}