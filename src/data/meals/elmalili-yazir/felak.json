{
  "surahNumber": "113",
  "surahName": "Felâk Suresi",
  "ayats": [
    {
      "ayahNumber": "1",
      "ayahText": "De ki: \"Ben, ağaran sabahın Rabbine sığınırım,"
    },
    {
      "ayahNumber": "2",
      "ayahText": "Yarattığı şeylerin şerrinden,"
    },
    {
      "ayahNumber": "3",
      "ayahText": "Karanlığı çöktüğü zaman gecenin şerrinden,"
    },
    {
      "ayahNumber": "4",
      "ayahText": "Ve düğümlere üfleyen büyücülerin şerrinden,"
    },
    {
      "ayahNumber": "5",
      "ayahText": "Ve hased ettiği zaman hasetçinin şerrinden."
    }
  ]
}