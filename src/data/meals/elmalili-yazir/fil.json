{
  "surahNumber": "105",
  "surahName": "Fîl Suresi",
  "ayats": [
    {
      "ayahNumber": "1",
      "ayahText": "Görmedin mi Rabb'in fil sahiplerine ne yaptı?"
    },
    {
      "ayahNumber": "2",
      "ayahText": "Onların tuzaklarını boşa çıkarmadı mı?"
    },
    {
      "ayahNumber": "3",
      "ayahText": "Üzerlerine sürü sürü kuşlar gönderdi."
    },
    {
      "ayahNumber": "4",
      "ayahText": "Onlara çamurdan sertleşmiş taşlar atıyorlardı."
    },
    {
      "ayahNumber": "5",
      "ayahText": "Ve onları, yenilmiş ekin yaprağı gibi yaptı."
    }
  ]
}