{
  "surahNumber": "114",
  "surahName": "Nâs Suresi",
  "ayats": [
    {
      "ayahNumber": "1",
      "ayahText": "De ki: Sığınırım ben insanların Rabbine,"
    },
    {
      "ayahNumber": "2",
      "ayahText": "İnsanların hükümdârına,"
    },
    {
      "ayahNumber": "3",
      "ayahText": "İnsanların ilâhına,"
    },
    {
      "ayahNumber": "4",
      "ayahText": "O sinsi vesvesecinin şerrinden."
    },
    {
      "ayahNumber": "5",
      "ayahText": "O ki, insanların göğüslerine vesveseler fısıldar."
    },
    {
      "ayahNumber": "6",
      "ayahText": "Gerek cinlerden, gerek insanlardan."
    }
  ]
}