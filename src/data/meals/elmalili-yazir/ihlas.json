  "surahNumber": "112",
  "surahName": "İhlâs Suresi",
  "ayats": [
    {
      "ayahNumber": "1",
      "ayahText": "De ki; O Allah bir tektir."
    },
    {
      "ayahNumber": "2",
      "ayahText": "Allah eksiksiz, sameddir (Bütün varlıklar O'na muhtaç, fakat O, hiç bir şeye muhtaç değildir"
    },
    {
      "ayahNumber": "3",
      "ayahText": "Doğurmadı ve doğurulmadı"
    },
    {
      "ayahNumber": "4",
      "ayahText": "O 'na bir denk de olmadı."
    }
  ]
