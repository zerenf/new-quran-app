{
  "abese": {
    "surahNumber": "80",
    "surahNameTr": "Abese Suresi",
    "ayats": [
      {
        "ayahNumber": "1",
        "ayahText": "`abese vetevellâ."
      },
      {
        "ayahNumber": "2",
        "ayahText": "en câehü-l'a`mâ."
      },
      {
        "ayahNumber": "3",
        "ayahText": "vemâ yüdrîke le`allehû yezzekkâ."
      },
      {
        "ayahNumber": "4",
        "ayahText": "ev yeẕẕekkeru fetenfe`ahü-ẕẕikrâ."
      },
      {
        "ayahNumber": "5",
        "ayahText": "emmâ meni-stagnâ."
      },
      {
        "ayahNumber": "6",
        "ayahText": "feente lehû teṣaddâ."
      },
      {
        "ayahNumber": "7",
        "ayahText": "vemâ `aleyke ellâ yezzekkâ."
      },
      {
        "ayahNumber": "8",
        "ayahText": "veemmâ men câeke yes`â."
      },
      {
        "ayahNumber": "9",
        "ayahText": "vehüve yaḫşâ."
      },
      {
        "ayahNumber": "10",
        "ayahText": "feente `anhü telehhâ."
      },
      {
        "ayahNumber": "11",
        "ayahText": "kellâ innehâ teẕkirah."
      },
      {
        "ayahNumber": "12",
        "ayahText": "femen şâe ẕekerah."
      },
      {
        "ayahNumber": "13",
        "ayahText": "fî ṣuḥufim mükerrameh."
      },
      {
        "ayahNumber": "14",
        "ayahText": "merfû`atim müṭahherah."
      },
      {
        "ayahNumber": "15",
        "ayahText": "bieydî seferah."
      },
      {
        "ayahNumber": "16",
        "ayahText": "kirâmim berarah."
      },
      {
        "ayahNumber": "17",
        "ayahText": "ḳutile-l'insânü mâ ekferah."
      },
      {
        "ayahNumber": "18",
        "ayahText": "min eyyi şey'in ḫaleḳah."
      },
      {
        "ayahNumber": "19",
        "ayahText": "min nuṭfeh. ḫaleḳahû feḳadderah."
      },
      {
        "ayahNumber": "20",
        "ayahText": "ŝümme-ssebîle yesserah."
      },
      {
        "ayahNumber": "21",
        "ayahText": "ŝümme emâtehû feaḳberah."
      },
      {
        "ayahNumber": "22",
        "ayahText": "ŝümme iẕâ şâe enşerah."
      },
      {
        "ayahNumber": "23",
        "ayahText": "kellâ lemmâ yaḳḍi mâ emerah."
      },
      {
        "ayahNumber": "24",
        "ayahText": "felyenżuri-l'insânü ilâ ṭa`âmih."
      },
      {
        "ayahNumber": "25",
        "ayahText": "ennâ ṣabebne-lmâe ṣabbâ."
      },
      {
        "ayahNumber": "26",
        "ayahText": "ŝümme şaḳaḳne-l'arḍa şeḳḳâ."
      },
      {
        "ayahNumber": "27",
        "ayahText": "feembetnâ fîhâ ḥabbâ."
      },
      {
        "ayahNumber": "28",
        "ayahText": "ve`inebev veḳaḍbâ."
      },
      {
        "ayahNumber": "29",
        "ayahText": "vezeytûnev venaḫlâ."
      },
      {
        "ayahNumber": "30",
        "ayahText": "veḥadâiḳa gulbâ."
      },
      {
        "ayahNumber": "31",
        "ayahText": "vefâkihetev veebbâ."
      },
      {
        "ayahNumber": "32",
        "ayahText": "metâ`al leküm velien`âmiküm."
      },
      {
        "ayahNumber": "33",
        "ayahText": "feiẕâ câeti-ṣṣâḫḫah."
      },
      {
        "ayahNumber": "34",
        "ayahText": "yevme yefirru-lmerü min eḫîh."
      },
      {
        "ayahNumber": "35",
        "ayahText": "veümmihî veebîh."
      },
      {
        "ayahNumber": "36",
        "ayahText": "veṣâḥibetihî vebenîh."
      },
      {
        "ayahNumber": "37",
        "ayahText": "likülli-mriim minhüm yevmeiẕin şe'nüy yugnîh."
      },
      {
        "ayahNumber": "38",
        "ayahText": "vucûhüy yevmeiẕim müsfirah."
      },
      {
        "ayahNumber": "39",
        "ayahText": "ḍâḥiketüm müstebşirah."
      },
      {
        "ayahNumber": "40",
        "ayahText": "vevucûhüy yevmeiẕin `aleyhâ gaberah."
      },
      {
        "ayahNumber": "41",
        "ayahText": "terheḳuhâ ḳaterah."
      },
      {
        "ayahNumber": "42",
        "ayahText": "ülâike hümü-lkeferatü-lfecerah."
      }
    ]
  }
}