{
  "surahNumber": "90",
  "surahName": "Beled Suresi",
  "ayats": [
    {
      "ayahNumber": "1",
      "ayahText": "lâ uḳsimü bihâẕe-lbeled."
    },
    {
      "ayahNumber": "2",
      "ayahText": "veente ḥillüm bihâẕe-lbeled."
    },
    {
      "ayahNumber": "3",
      "ayahText": "vevâlidiv vemâ veled."
    },
    {
      "ayahNumber": "4",
      "ayahText": "leḳad ḫalaḳne-l'insâne fî kebed."
    },
    {
      "ayahNumber": "5",
      "ayahText": "eyaḥsebü el ley yaḳdira `aleyhi eḥad."
    },
    {
      "ayahNumber": "6",
      "ayahText": "yeḳûlü ehlektü mâlel lübedâ."
    },
    {
      "ayahNumber": "7",
      "ayahText": "eyaḥsebü el lem yerahû eḥad."
    },
    {
      "ayahNumber": "8",
      "ayahText": "elem nec`al lehû `ayneyn."
    },
    {
      "ayahNumber": "9",
      "ayahText": "velisânev veşefeteyn."
    },
    {
      "ayahNumber": "10",
      "ayahText": "vehedeynâhü-nnecdeyn."
    },
    {
      "ayahNumber": "11",
      "ayahText": "fele-ḳteḥame-l`aḳabeh."
    },
    {
      "ayahNumber": "12",
      "ayahText": "vemâ edrâke me-l`aḳabeh."
    },
    {
      "ayahNumber": "13",
      "ayahText": "fekkü raḳabeh."
    },
    {
      "ayahNumber": "14",
      "ayahText": "ev iṭ`âmün fî yevmin ẕî mesgabeh."
    },
    {
      "ayahNumber": "15",
      "ayahText": "yetîmen ẕâ maḳrabeh."
    },
    {
      "ayahNumber": "16",
      "ayahText": "ev miskînen ẕâ metrabeh."
    },
    {
      "ayahNumber": "17",
      "ayahText": "ŝümme kâne mine-lleẕîne âmenû vetevâṣav biṣṣabri vetevâṣav bilmerḥameh."
    },
    {
      "ayahNumber": "18",
      "ayahText": "ülâike aṣḥâbü-lmeymeneh."
    },
    {
      "ayahNumber": "19",
      "ayahText": "velleẕîne keferû biâyâtinâ hüm aṣḥâbü-lmeş'emeh."
    },
    {
      "ayahNumber": "20",
      "ayahText": "`aleyhim nârum mü'ṣadeh."
    }
  ]
}