{
  "nas": {
    "surahNumber": "114",
    "surahNameTr": "Nâs Suresi",
    "ayats": [
      {
        "ayahNumber": "1",
        "ayahText": "De ki: İnsanların Rabbine,"
      },
      {
        "ayahNumber": "2",
        "ayahText": "İnsanların yegane Hükümdarına,"
      },
      {
        "ayahNumber": "3",
        "ayahText": "İnsanların İlahına sığınırım:"
      },
      {
        "ayahNumber": "4",
        "ayahText": "O sinsi şeytanın şerrinden"
      },
      {
        "ayahNumber": "5",
        "ayahText": "O ki insanların kalplerine vesvese verir,"
      },
      {
        "ayahNumber": "6",
        "ayahText": "O şeytan, cinlerden de olur, insanlardan da olur. [6,112]"
      }
    ]
  }
}