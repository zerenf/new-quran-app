{
  "surahNumber": "95",
  "surahName": "Tîn Suresi",
  "ayats": [
    {
      "ayahNumber": "1",
      "ayahText": "İncire ve zeytine andolsun,"
    },
    {
      "ayahNumber": "2",
      "ayahText": "Sina dağına,"
    },
    {
      "ayahNumber": "3",
      "ayahText": "Ve şu emin beldeye (güvenilir şehre)."
    },
    {
      "ayahNumber": "4",
      "ayahText": "Doğrusu, Biz insanı en güzel bir biçimde yarattık."
    },
    {
      "ayahNumber": "5",
      "ayahText": "Sonra aşağıların aşağısına çevirdik."
    },
    {
      "ayahNumber": "6",
      "ayahText": "Ancak iman edip salih amellerde bulunanlar başka; onlar için kesintisi olmayan bir ecir vardır."
    },
    {
      "ayahNumber": "7",
      "ayahText": "Öyleyse bundan sonra, hangi şey sana dini yalanlatabilir?"
    },
    {
      "ayahNumber": "8",
      "ayahText": "Allah hükmedenlerin hakimi değil midir?"
    }
  ]
}