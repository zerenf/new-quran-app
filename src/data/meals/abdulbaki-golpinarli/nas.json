{
  "nas": {
    "surahNumber": "114",
    "surahNameTr": "Nâs Suresi",
    "ayats": [
      {
        "ayahNumber": "1",
        "ayahText": "De ki: Sığınırım insanların Rabbine."
      },
      {
        "ayahNumber": "2",
        "ayahText": "İnsanların sahibine."
      },
      {
        "ayahNumber": "3",
        "ayahText": "İnsanların mabuduna."
      },
      {
        "ayahNumber": "4",
        "ayahText": "Gizlice, sinsisinsi vesveseler verenin şerrinden."
      },
      {
        "ayahNumber": "5",
        "ayahText": "Öylesine ki insanların gönüllerine vesveseler sokar."
      },
      {
        "ayahNumber": "6",
        "ayahText": "Cinden olsun, insandan olsun, bu çeşit kişilerin şerrinden."
      }
    ]
  }
}