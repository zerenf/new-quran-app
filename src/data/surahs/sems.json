{
	"sems": {
		"surahNumber": "91",
		"surahName": "الشمس",
		"surahNameTr": "Şems",
		"ayats": [
			{
				"ayahNumber": "1",
				"ayahText": "وَالشَّمْسِ وَضُحَاهَا"
			},
			{
				"ayahNumber": "2",
				"ayahText": "وَالْقَمَرِ إِذَا تَلَاهَا"
			},
			{
				"ayahNumber": "3",
				"ayahText": "وَالنَّهَارِ إِذَا جَلَّاهَا"
			},
			{
				"ayahNumber": "4",
				"ayahText": "وَاللَّيْلِ إِذَا يَغْشَاهَا"
			},
			{
				"ayahNumber": "5",
				"ayahText": "وَالسَّمَاءِ وَمَا بَنَاهَا"
			},
			{
				"ayahNumber": "6",
				"ayahText": "وَالْأَرْضِ وَمَا طَحَاهَا"
			},
			{
				"ayahNumber": "7",
				"ayahText": "وَنَفْسٍ وَمَا سَوَّاهَا"
			},
			{
				"ayahNumber": "8",
				"ayahText": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَاهَا"
			},
			{
				"ayahNumber": "9",
				"ayahText": "قَدْ أَفْلَحَ مَن زَكَّاهَا"
			},
			{
				"ayahNumber": "10",
				"ayahText": "وَقَدْ خَابَ مَن دَسَّاهَا"
			},
			{
				"ayahNumber": "11",
				"ayahText": "كَذَّبَتْ ثَمُودُ بِطَغْوَاهَا"
			},
			{
				"ayahNumber": "12",
				"ayahText": "إِذِ انبَعَثَ أَشْقَاهَا"
			},
			{
				"ayahNumber": "13",
				"ayahText": "فَقَالَ لَهُمْ رَسُولُ اللَّهِ نَاقَةَ اللَّهِ وَسُقْيَاهَا"
			},
			{
				"ayahNumber": "14",
				"ayahText": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُم بِذَنبِهِمْ فَسَوَّاهَا"
			},
			{
				"ayahNumber": "15",
				"ayahText": "وَلَا يَخَافُ عُقْبَاهَا"
			}
		]
	}
}
