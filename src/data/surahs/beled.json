{
	"beled": {
		"surahNumber": "90",
		"surahName": "البلد",
		"surahNameTr": "Beled",
		"ayats": [
			{
				"ayahNumber": "1",
				"ayahText": "لَا أُقْسِمُ بِهَـٰذَا الْبَلَدِ"
			},
			{
				"ayahNumber": "2",
				"ayahText": "وَأَنتَ حِلٌّ بِهَـٰذَا الْبَلَدِ"
			},
			{
				"ayahNumber": "3",
				"ayahText": "وَوَالِدٍ وَمَا وَلَدَ"
			},
			{
				"ayahNumber": "4",
				"ayahText": "لَقَدْ خَلَقْنَا الْإِنسَانَ فِي كَبَدٍ"
			},
			{
				"ayahNumber": "5",
				"ayahText": "أَيَحْسَبُ أَن لَّن يَقْدِرَ عَلَيْهِ أَحَدٌ"
			},
			{
				"ayahNumber": "6",
				"ayahText": "يَقُولُ أَهْلَكْتُ مَالًا لُّبَدًا"
			},
			{
				"ayahNumber": "7",
				"ayahText": "أَيَحْسَبُ أَن لَّمْ يَرَهُ أَحَدٌ"
			},
			{
				"ayahNumber": "8",
				"ayahText": "أَلَمْ نَجْعَل لَّهُ عَيْنَيْنِ"
			},
			{
				"ayahNumber": "9",
				"ayahText": "وَلِسَانًا وَشَفَتَيْنِ"
			},
			{
				"ayahNumber": "10",
				"ayahText": "وَهَدَيْنَاهُ النَّجْدَيْنِ"
			},
			{
				"ayahNumber": "11",
				"ayahText": "فَلَا اقْتَحَمَ الْعَقَبَةَ"
			},
			{
				"ayahNumber": "12",
				"ayahText": "وَمَا أَدْرَاكَ مَا الْعَقَبَةُ"
			},
			{
				"ayahNumber": "13",
				"ayahText": "فَكُّ رَقَبَةٍ"
			},
			{
				"ayahNumber": "14",
				"ayahText": "أَوْ إِطْعَامٌ فِي يَوْمٍ ذِي مَسْغَبَةٍ"
			},
			{
				"ayahNumber": "15",
				"ayahText": "يَتِيمًا ذَا مَقْرَبَةٍ"
			},
			{
				"ayahNumber": "16",
				"ayahText": "أَوْ مِسْكِينًا ذَا مَتْرَبَةٍ"
			},
			{
				"ayahNumber": "17",
				"ayahText": "ثُمَّ كَانَ مِنَ الَّذِينَ آمَنُوا وَتَوَاصَوْا بِالصَّبْرِ وَتَوَاصَوْا بِالْمَرْحَمَةِ"
			},
			{
				"ayahNumber": "18",
				"ayahText": "أُولَـٰئِكَ أَصْحَابُ الْمَيْمَنَةِ"
			},
			{
				"ayahNumber": "19",
				"ayahText": "وَالَّذِينَ كَفَرُوا بِآيَاتِنَا هُمْ أَصْحَابُ الْمَشْأَمَةِ"
			},
			{
				"ayahNumber": "20",
				"ayahText": "عَلَيْهِمْ نَارٌ مُّؤْصَدَةٌ"
			}
		]
	}
}
