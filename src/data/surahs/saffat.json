{
	"saffat": {
		"surahNumber": "37",
		"surahName": "الصافات",
		"surahNameTr": "Sâffât",
		"ayats": [
			{
				"ayahNumber": "1",
				"ayahText": "وَالصَّافَّاتِ صَفًّا"
			},
			{
				"ayahNumber": "2",
				"ayahText": "فَالزَّاجِرَاتِ زَجْرًا"
			},
			{
				"ayahNumber": "3",
				"ayahText": "فَالتَّالِيَاتِ ذِكْرًا"
			},
			{
				"ayahNumber": "4",
				"ayahText": "إِنَّ إِلَـٰهَكُمْ لَوَاحِدٌ"
			},
			{
				"ayahNumber": "5",
				"ayahText": "رَّبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا وَرَبُّ الْمَشَارِقِ"
			},
			{
				"ayahNumber": "6",
				"ayahText": "إِنَّا زَيَّنَّا السَّمَاءَ الدُّنْيَا بِزِينَةٍ الْكَوَاكِبِ"
			},
			{
				"ayahNumber": "7",
				"ayahText": "وَحِفْظًا مِّن كُلِّ شَيْطَانٍ مَّارِدٍ"
			},
			{
				"ayahNumber": "8",
				"ayahText": "لَّا يَسَّمَّعُونَ إِلَى الْمَلَإِ الْأَعْلَىٰ وَيُقْذَفُونَ مِن كُلِّ جَانِبٍ"
			},
			{
				"ayahNumber": "9",
				"ayahText": "دُحُورًا ۖ وَلَهُمْ عَذَابٌ وَاصِبٌ"
			},
			{
				"ayahNumber": "10",
				"ayahText": "إِلَّا مَنْ خَطِفَ الْخَطْفَةَ فَأَتْبَعَهُ شِهَابٌ ثَاقِبٌ"
			},
			{
				"ayahNumber": "11",
				"ayahText": "فَاسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقًا أَم مَّنْ خَلَقْنَا ۚ إِنَّا خَلَقْنَاهُم مِّن طِينٍ لَّازِبٍ"
			},
			{
				"ayahNumber": "12",
				"ayahText": "بَلْ عَجِبْتَ وَيَسْخَرُونَ"
			},
			{
				"ayahNumber": "13",
				"ayahText": "وَإِذَا ذُكِّرُوا لَا يَذْكُرُونَ"
			},
			{
				"ayahNumber": "14",
				"ayahText": "وَإِذَا رَأَوْا آيَةً يَسْتَسْخِرُونَ"
			},
			{
				"ayahNumber": "15",
				"ayahText": "وَقَالُوا إِنْ هَـٰذَا إِلَّا سِحْرٌ مُّبِينٌ"
			},
			{
				"ayahNumber": "16",
				"ayahText": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَبْعُوثُونَ"
			},
			{
				"ayahNumber": "17",
				"ayahText": "أَوَآبَاؤُنَا الْأَوَّلُونَ"
			},
			{
				"ayahNumber": "18",
				"ayahText": "قُلْ نَعَمْ وَأَنتُمْ دَاخِرُونَ"
			},
			{
				"ayahNumber": "19",
				"ayahText": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ فَإِذَا هُمْ يَنظُرُونَ"
			},
			{
				"ayahNumber": "20",
				"ayahText": "وَقَالُوا يَا وَيْلَنَا هَـٰذَا يَوْمُ الدِّينِ"
			},
			{
				"ayahNumber": "21",
				"ayahText": "هَـٰذَا يَوْمُ الْفَصْلِ الَّذِي كُنتُم بِهِ تُكَذِّبُونَ"
			},
			{
				"ayahNumber": "22",
				"ayahText": "احْشُرُوا الَّذِينَ ظَلَمُوا وَأَزْوَاجَهُمْ وَمَا كَانُوا يَعْبُدُونَ"
			},
			{
				"ayahNumber": "23",
				"ayahText": "مِن دُونِ اللَّهِ فَاهْدُوهُمْ إِلَىٰ صِرَاطِ الْجَحِيمِ"
			},
			{
				"ayahNumber": "24",
				"ayahText": "وَقِفُوهُمْ ۖ إِنَّهُم مَّسْئُولُونَ"
			},
			{
				"ayahNumber": "25",
				"ayahText": "مَا لَكُمْ لَا تَنَاصَرُونَ"
			},
			{
				"ayahNumber": "26",
				"ayahText": "بَلْ هُمُ الْيَوْمَ مُسْتَسْلِمُونَ"
			},
			{
				"ayahNumber": "27",
				"ayahText": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَاءَلُونَ"
			},
			{
				"ayahNumber": "28",
				"ayahText": "قَالُوا إِنَّكُمْ كُنتُمْ تَأْتُونَنَا عَنِ الْيَمِينِ"
			},
			{
				"ayahNumber": "29",
				"ayahText": "قَالُوا بَل لَّمْ تَكُونُوا مُؤْمِنِينَ"
			},
			{
				"ayahNumber": "30",
				"ayahText": "وَمَا كَانَ لَنَا عَلَيْكُم مِّن سُلْطَانٍ ۖ بَلْ كُنتُمْ قَوْمًا طَاغِينَ"
			},
			{
				"ayahNumber": "31",
				"ayahText": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَا ۖ إِنَّا لَذَائِقُونَ"
			},
			{
				"ayahNumber": "32",
				"ayahText": "فَأَغْوَيْنَاكُمْ إِنَّا كُنَّا غَاوِينَ"
			},
			{
				"ayahNumber": "33",
				"ayahText": "فَإِنَّهُمْ يَوْمَئِذٍ فِي الْعَذَابِ مُشْتَرِكُونَ"
			},
			{
				"ayahNumber": "34",
				"ayahText": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ"
			},
			{
				"ayahNumber": "35",
				"ayahText": "إِنَّهُمْ كَانُوا إِذَا قِيلَ لَهُمْ لَا إِلَـٰهَ إِلَّا اللَّهُ يَسْتَكْبِرُونَ"
			},
			{
				"ayahNumber": "36",
				"ayahText": "وَيَقُولُونَ أَئِنَّا لَتَارِكُو آلِهَتِنَا لِشَاعِرٍ مَّجْنُونٍ"
			},
			{
				"ayahNumber": "37",
				"ayahText": "بَلْ جَاءَ بِالْحَقِّ وَصَدَّقَ الْمُرْسَلِينَ"
			},
			{
				"ayahNumber": "38",
				"ayahText": "إِنَّكُمْ لَذَائِقُو الْعَذَابِ الْأَلِيمِ"
			},
			{
				"ayahNumber": "39",
				"ayahText": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنتُمْ تَعْمَلُونَ"
			},
			{
				"ayahNumber": "40",
				"ayahText": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
			},
			{
				"ayahNumber": "41",
				"ayahText": "أُولَـٰئِكَ لَهُمْ رِزْقٌ مَّعْلُومٌ"
			},
			{
				"ayahNumber": "42",
				"ayahText": "فَوَاكِهُ ۖ وَهُم مُّكْرَمُونَ"
			},
			{
				"ayahNumber": "43",
				"ayahText": "فِي جَنَّاتِ النَّعِيمِ"
			},
			{
				"ayahNumber": "44",
				"ayahText": "عَلَىٰ سُرُرٍ مُّتَقَابِلِينَ"
			},
			{
				"ayahNumber": "45",
				"ayahText": "يُطَافُ عَلَيْهِم بِكَأْسٍ مِّن مَّعِينٍ"
			},
			{
				"ayahNumber": "46",
				"ayahText": "بَيْضَاءَ لَذَّةٍ لِّلشَّارِبِينَ"
			},
			{
				"ayahNumber": "47",
				"ayahText": "لَا فِيهَا غَوْلٌ وَلَا هُمْ عَنْهَا يُنزَفُونَ"
			},
			{
				"ayahNumber": "48",
				"ayahText": "وَعِندَهُمْ قَاصِرَاتُ الطَّرْفِ عِينٌ"
			},
			{
				"ayahNumber": "49",
				"ayahText": "كَأَنَّهُنَّ بَيْضٌ مَّكْنُونٌ"
			},
			{
				"ayahNumber": "50",
				"ayahText": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَاءَلُونَ"
			},
			{
				"ayahNumber": "51",
				"ayahText": "قَالَ قَائِلٌ مِّنْهُمْ إِنِّي كَانَ لِي قَرِينٌ"
			},
			{
				"ayahNumber": "52",
				"ayahText": "يَقُولُ أَإِنَّكَ لَمِنَ الْمُصَدِّقِينَ"
			},
			{
				"ayahNumber": "53",
				"ayahText": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَدِينُونَ"
			},
			{
				"ayahNumber": "54",
				"ayahText": "قَالَ هَلْ أَنتُم مُّطَّلِعُونَ"
			},
			{
				"ayahNumber": "55",
				"ayahText": "فَاطَّلَعَ فَرَآهُ فِي سَوَاءِ الْجَحِيمِ"
			},
			{
				"ayahNumber": "56",
				"ayahText": "قَالَ تَاللَّهِ إِن كِدتَّ لَتُرْدِينِ"
			},
			{
				"ayahNumber": "57",
				"ayahText": "وَلَوْلَا نِعْمَةُ رَبِّي لَكُنتُ مِنَ الْمُحْضَرِينَ"
			},
			{
				"ayahNumber": "58",
				"ayahText": "أَفَمَا نَحْنُ بِمَيِّتِينَ"
			},
			{
				"ayahNumber": "59",
				"ayahText": "إِلَّا مَوْتَتَنَا الْأُولَىٰ وَمَا نَحْنُ بِمُعَذَّبِينَ"
			},
			{
				"ayahNumber": "60",
				"ayahText": "إِنَّ هَـٰذَا لَهُوَ الْفَوْزُ الْعَظِيمُ"
			},
			{
				"ayahNumber": "61",
				"ayahText": "لِمِثْلِ هَـٰذَا فَلْيَعْمَلِ الْعَامِلُونَ"
			},
			{
				"ayahNumber": "62",
				"ayahText": "أَذَٰلِكَ خَيْرٌ نُّزُلًا أَمْ شَجَرَةُ الزَّقُّومِ"
			},
			{
				"ayahNumber": "63",
				"ayahText": "إِنَّا جَعَلْنَاهَا فِتْنَةً لِّلظَّالِمِينَ"
			},
			{
				"ayahNumber": "64",
				"ayahText": "إِنَّهَا شَجَرَةٌ تَخْرُجُ فِي أَصْلِ الْجَحِيمِ"
			},
			{
				"ayahNumber": "65",
				"ayahText": "طَلْعُهَا كَأَنَّهُ رُءُوسُ الشَّيَاطِينِ"
			},
			{
				"ayahNumber": "66",
				"ayahText": "فَإِنَّهُمْ لَآكِلُونَ مِنْهَا فَمَالِئُونَ مِنْهَا الْبُطُونَ"
			},
			{
				"ayahNumber": "67",
				"ayahText": "ثُمَّ إِنَّ لَهُمْ عَلَيْهَا لَشَوْبًا مِّنْ حَمِيمٍ"
			},
			{
				"ayahNumber": "68",
				"ayahText": "ثُمَّ إِنَّ مَرْجِعَهُمْ لَإِلَى الْجَحِيمِ"
			},
			{
				"ayahNumber": "69",
				"ayahText": "إِنَّهُمْ أَلْفَوْا آبَاءَهُمْ ضَالِّينَ"
			},
			{
				"ayahNumber": "70",
				"ayahText": "فَهُمْ عَلَىٰ آثَارِهِمْ يُهْرَعُونَ"
			},
			{
				"ayahNumber": "71",
				"ayahText": "وَلَقَدْ ضَلَّ قَبْلَهُمْ أَكْثَرُ الْأَوَّلِينَ"
			},
			{
				"ayahNumber": "72",
				"ayahText": "وَلَقَدْ أَرْسَلْنَا فِيهِم مُّنذِرِينَ"
			},
			{
				"ayahNumber": "73",
				"ayahText": "فَانظُرْ كَيْفَ كَانَ عَاقِبَةُ الْمُنذَرِينَ"
			},
			{
				"ayahNumber": "74",
				"ayahText": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
			},
			{
				"ayahNumber": "75",
				"ayahText": "وَلَقَدْ نَادَانَا نُوحٌ فَلَنِعْمَ الْمُجِيبُونَ"
			},
			{
				"ayahNumber": "76",
				"ayahText": "وَنَجَّيْنَاهُ وَأَهْلَهُ مِنَ الْكَرْبِ الْعَظِيمِ"
			},
			{
				"ayahNumber": "77",
				"ayahText": "وَجَعَلْنَا ذُرِّيَّتَهُ هُمُ الْبَاقِينَ"
			},
			{
				"ayahNumber": "78",
				"ayahText": "وَتَرَكْنَا عَلَيْهِ فِي الْآخِرِينَ"
			},
			{
				"ayahNumber": "79",
				"ayahText": "سَلَامٌ عَلَىٰ نُوحٍ فِي الْعَالَمِينَ"
			},
			{
				"ayahNumber": "80",
				"ayahText": "إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
			},
			{
				"ayahNumber": "81",
				"ayahText": "إِنَّهُ مِنْ عِبَادِنَا الْمُؤْمِنِينَ"
			},
			{
				"ayahNumber": "82",
				"ayahText": "ثُمَّ أَغْرَقْنَا الْآخَرِينَ"
			},
			{
				"ayahNumber": "83",
				"ayahText": "وَإِنَّ مِن شِيعَتِهِ لَإِبْرَاهِيمَ"
			},
			{
				"ayahNumber": "84",
				"ayahText": "إِذْ جَاءَ رَبَّهُ بِقَلْبٍ سَلِيمٍ"
			},
			{
				"ayahNumber": "85",
				"ayahText": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِ مَاذَا تَعْبُدُونَ"
			},
			{
				"ayahNumber": "86",
				"ayahText": "أَئِفْكًا آلِهَةً دُونَ اللَّهِ تُرِيدُونَ"
			},
			{
				"ayahNumber": "87",
				"ayahText": "فَمَا ظَنُّكُم بِرَبِّ الْعَالَمِينَ"
			},
			{
				"ayahNumber": "88",
				"ayahText": "فَنَظَرَ نَظْرَةً فِي النُّجُومِ"
			},
			{
				"ayahNumber": "89",
				"ayahText": "فَقَالَ إِنِّي سَقِيمٌ"
			},
			{
				"ayahNumber": "90",
				"ayahText": "فَتَوَلَّوْا عَنْهُ مُدْبِرِينَ"
			},
			{
				"ayahNumber": "91",
				"ayahText": "فَرَاغَ إِلَىٰ آلِهَتِهِمْ فَقَالَ أَلَا تَأْكُلُونَ"
			},
			{
				"ayahNumber": "92",
				"ayahText": "مَا لَكُمْ لَا تَنطِقُونَ"
			},
			{
				"ayahNumber": "93",
				"ayahText": "فَرَاغَ عَلَيْهِمْ ضَرْبًا بِالْيَمِينِ"
			},
			{
				"ayahNumber": "94",
				"ayahText": "فَأَقْبَلُوا إِلَيْهِ يَزِفُّونَ"
			},
			{
				"ayahNumber": "95",
				"ayahText": "قَالَ أَتَعْبُدُونَ مَا تَنْحِتُونَ"
			},
			{
				"ayahNumber": "96",
				"ayahText": "وَاللَّهُ خَلَقَكُمْ وَمَا تَعْمَلُونَ"
			},
			{
				"ayahNumber": "97",
				"ayahText": "قَالُوا ابْنُوا لَهُ بُنْيَانًا فَأَلْقُوهُ فِي الْجَحِيمِ"
			},
			{
				"ayahNumber": "98",
				"ayahText": "فَأَرَادُوا بِهِ كَيْدًا فَجَعَلْنَاهُمُ الْأَسْفَلِينَ"
			},
			{
				"ayahNumber": "99",
				"ayahText": "وَقَالَ إِنِّي ذَاهِبٌ إِلَىٰ رَبِّي سَيَهْدِينِ"
			},
			{
				"ayahNumber": "100",
				"ayahText": "رَبِّ هَبْ لِي مِنَ الصَّالِحِينَ"
			},
			{
				"ayahNumber": "101",
				"ayahText": "فَبَشَّرْنَاهُ بِغُلَامٍ حَلِيمٍ"
			},
			{
				"ayahNumber": "102",
				"ayahText": "فَلَمَّا بَلَغَ مَعَهُ السَّعْيَ قَالَ يَا بُنَيَّ إِنِّي أَرَىٰ فِي الْمَنَامِ أَنِّي أَذْبَحُكَ فَانظُرْ مَاذَا تَرَىٰ ۚ قَالَ يَا أَبَتِ افْعَلْ مَا تُؤْمَرُ ۖ سَتَجِدُنِي إِن شَاءَ اللَّهُ مِنَ الصَّابِرِينَ"
			},
			{
				"ayahNumber": "103",
				"ayahText": "فَلَمَّا أَسْلَمَا وَتَلَّهُ لِلْجَبِينِ"
			},
			{
				"ayahNumber": "104",
				"ayahText": "وَنَادَيْنَاهُ أَن يَا إِبْرَاهِيمُ"
			},
			{
				"ayahNumber": "105",
				"ayahText": "قَدْ صَدَّقْتَ الرُّؤْيَا ۚ إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
			},
			{
				"ayahNumber": "106",
				"ayahText": "إِنَّ هَـٰذَا لَهُوَ الْبَلَاءُ الْمُبِينُ"
			},
			{
				"ayahNumber": "107",
				"ayahText": "وَفَدَيْنَاهُ بِذِبْحٍ عَظِيمٍ"
			},
			{
				"ayahNumber": "108",
				"ayahText": "وَتَرَكْنَا عَلَيْهِ فِي الْآخِرِينَ"
			},
			{
				"ayahNumber": "109",
				"ayahText": "سَلَامٌ عَلَىٰ إِبْرَاهِيمَ"
			},
			{
				"ayahNumber": "110",
				"ayahText": "كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
			},
			{
				"ayahNumber": "111",
				"ayahText": "إِنَّهُ مِنْ عِبَادِنَا الْمُؤْمِنِينَ"
			},
			{
				"ayahNumber": "112",
				"ayahText": "وَبَشَّرْنَاهُ بِإِسْحَاقَ نَبِيًّا مِّنَ الصَّالِحِينَ"
			},
			{
				"ayahNumber": "113",
				"ayahText": "وَبَارَكْنَا عَلَيْهِ وَعَلَىٰ إِسْحَاقَ ۚ وَمِن ذُرِّيَّتِهِمَا مُحْسِنٌ وَظَالِمٌ لِّنَفْسِهِ مُبِينٌ"
			},
			{
				"ayahNumber": "114",
				"ayahText": "وَلَقَدْ مَنَنَّا عَلَىٰ مُوسَىٰ وَهَارُونَ"
			},
			{
				"ayahNumber": "115",
				"ayahText": "وَنَجَّيْنَاهُمَا وَقَوْمَهُمَا مِنَ الْكَرْبِ الْعَظِيمِ"
			},
			{
				"ayahNumber": "116",
				"ayahText": "وَنَصَرْنَاهُمْ فَكَانُوا هُمُ الْغَالِبِينَ"
			},
			{
				"ayahNumber": "117",
				"ayahText": "وَآتَيْنَاهُمَا الْكِتَابَ الْمُسْتَبِينَ"
			},
			{
				"ayahNumber": "118",
				"ayahText": "وَهَدَيْنَاهُمَا الصِّرَاطَ الْمُسْتَقِيمَ"
			},
			{
				"ayahNumber": "119",
				"ayahText": "وَتَرَكْنَا عَلَيْهِمَا فِي الْآخِرِينَ"
			},
			{
				"ayahNumber": "120",
				"ayahText": "سَلَامٌ عَلَىٰ مُوسَىٰ وَهَارُونَ"
			},
			{
				"ayahNumber": "121",
				"ayahText": "إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
			},
			{
				"ayahNumber": "122",
				"ayahText": "إِنَّهُمَا مِنْ عِبَادِنَا الْمُؤْمِنِينَ"
			},
			{
				"ayahNumber": "123",
				"ayahText": "وَإِنَّ إِلْيَاسَ لَمِنَ الْمُرْسَلِينَ"
			},
			{
				"ayahNumber": "124",
				"ayahText": "إِذْ قَالَ لِقَوْمِهِ أَلَا تَتَّقُونَ"
			},
			{
				"ayahNumber": "125",
				"ayahText": "أَتَدْعُونَ بَعْلًا وَتَذَرُونَ أَحْسَنَ الْخَالِقِينَ"
			},
			{
				"ayahNumber": "126",
				"ayahText": "اللَّهَ رَبَّكُمْ وَرَبَّ آبَائِكُمُ الْأَوَّلِينَ"
			},
			{
				"ayahNumber": "127",
				"ayahText": "فَكَذَّبُوهُ فَإِنَّهُمْ لَمُحْضَرُونَ"
			},
			{
				"ayahNumber": "128",
				"ayahText": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
			},
			{
				"ayahNumber": "129",
				"ayahText": "وَتَرَكْنَا عَلَيْهِ فِي الْآخِرِينَ"
			},
			{
				"ayahNumber": "130",
				"ayahText": "سَلَامٌ عَلَىٰ إِلْ يَاسِينَ"
			},
			{
				"ayahNumber": "131",
				"ayahText": "إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
			},
			{
				"ayahNumber": "132",
				"ayahText": "إِنَّهُ مِنْ عِبَادِنَا الْمُؤْمِنِينَ"
			},
			{
				"ayahNumber": "133",
				"ayahText": "وَإِنَّ لُوطًا لَّمِنَ الْمُرْسَلِينَ"
			},
			{
				"ayahNumber": "134",
				"ayahText": "إِذْ نَجَّيْنَاهُ وَأَهْلَهُ أَجْمَعِينَ"
			},
			{
				"ayahNumber": "135",
				"ayahText": "إِلَّا عَجُوزًا فِي الْغَابِرِينَ"
			},
			{
				"ayahNumber": "136",
				"ayahText": "ثُمَّ دَمَّرْنَا الْآخَرِينَ"
			},
			{
				"ayahNumber": "137",
				"ayahText": "وَإِنَّكُمْ لَتَمُرُّونَ عَلَيْهِم مُّصْبِحِينَ"
			},
			{
				"ayahNumber": "138",
				"ayahText": "وَبِاللَّيْلِ ۗ أَفَلَا تَعْقِلُونَ"
			},
			{
				"ayahNumber": "139",
				"ayahText": "وَإِنَّ يُونُسَ لَمِنَ الْمُرْسَلِينَ"
			},
			{
				"ayahNumber": "140",
				"ayahText": "إِذْ أَبَقَ إِلَى الْفُلْكِ الْمَشْحُونِ"
			},
			{
				"ayahNumber": "141",
				"ayahText": "فَسَاهَمَ فَكَانَ مِنَ الْمُدْحَضِينَ"
			},
			{
				"ayahNumber": "142",
				"ayahText": "فَالْتَقَمَهُ الْحُوتُ وَهُوَ مُلِيمٌ"
			},
			{
				"ayahNumber": "143",
				"ayahText": "فَلَوْلَا أَنَّهُ كَانَ مِنَ الْمُسَبِّحِينَ"
			},
			{
				"ayahNumber": "144",
				"ayahText": "لَلَبِثَ فِي بَطْنِهِ إِلَىٰ يَوْمِ يُبْعَثُونَ"
			},
			{
				"ayahNumber": "145",
				"ayahText": "فَنَبَذْنَاهُ بِالْعَرَاءِ وَهُوَ سَقِيمٌ"
			},
			{
				"ayahNumber": "146",
				"ayahText": "وَأَنبَتْنَا عَلَيْهِ شَجَرَةً مِّن يَقْطِينٍ"
			},
			{
				"ayahNumber": "147",
				"ayahText": "وَأَرْسَلْنَاهُ إِلَىٰ مِائَةِ أَلْفٍ أَوْ يَزِيدُونَ"
			},
			{
				"ayahNumber": "148",
				"ayahText": "فَآمَنُوا فَمَتَّعْنَاهُمْ إِلَىٰ حِينٍ"
			},
			{
				"ayahNumber": "149",
				"ayahText": "فَاسْتَفْتِهِمْ أَلِرَبِّكَ الْبَنَاتُ وَلَهُمُ الْبَنُونَ"
			},
			{
				"ayahNumber": "150",
				"ayahText": "أَمْ خَلَقْنَا الْمَلَائِكَةَ إِنَاثًا وَهُمْ شَاهِدُونَ"
			},
			{
				"ayahNumber": "151",
				"ayahText": "أَلَا إِنَّهُم مِّنْ إِفْكِهِمْ لَيَقُولُونَ"
			},
			{
				"ayahNumber": "152",
				"ayahText": "وَلَدَ اللَّهُ وَإِنَّهُمْ لَكَاذِبُونَ"
			},
			{
				"ayahNumber": "153",
				"ayahText": "أَصْطَفَى الْبَنَاتِ عَلَى الْبَنِينَ"
			},
			{
				"ayahNumber": "154",
				"ayahText": "مَا لَكُمْ كَيْفَ تَحْكُمُونَ"
			},
			{
				"ayahNumber": "155",
				"ayahText": "أَفَلَا تَذَكَّرُونَ"
			},
			{
				"ayahNumber": "156",
				"ayahText": "أَمْ لَكُمْ سُلْطَانٌ مُّبِينٌ"
			},
			{
				"ayahNumber": "157",
				"ayahText": "فَأْتُوا بِكِتَابِكُمْ إِن كُنتُمْ صَادِقِينَ"
			},
			{
				"ayahNumber": "158",
				"ayahText": "وَجَعَلُوا بَيْنَهُ وَبَيْنَ الْجِنَّةِ نَسَبًا ۚ وَلَقَدْ عَلِمَتِ الْجِنَّةُ إِنَّهُمْ لَمُحْضَرُونَ"
			},
			{
				"ayahNumber": "159",
				"ayahText": "سُبْحَانَ اللَّهِ عَمَّا يَصِفُونَ"
			},
			{
				"ayahNumber": "160",
				"ayahText": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
			},
			{
				"ayahNumber": "161",
				"ayahText": "فَإِنَّكُمْ وَمَا تَعْبُدُونَ"
			},
			{
				"ayahNumber": "162",
				"ayahText": "مَا أَنتُمْ عَلَيْهِ بِفَاتِنِينَ"
			},
			{
				"ayahNumber": "163",
				"ayahText": "إِلَّا مَنْ هُوَ صَالِ الْجَحِيمِ"
			},
			{
				"ayahNumber": "164",
				"ayahText": "وَمَا مِنَّا إِلَّا لَهُ مَقَامٌ مَّعْلُومٌ"
			},
			{
				"ayahNumber": "165",
				"ayahText": "وَإِنَّا لَنَحْنُ الصَّافُّونَ"
			},
			{
				"ayahNumber": "166",
				"ayahText": "وَإِنَّا لَنَحْنُ الْمُسَبِّحُونَ"
			},
			{
				"ayahNumber": "167",
				"ayahText": "وَإِن كَانُوا لَيَقُولُونَ"
			},
			{
				"ayahNumber": "168",
				"ayahText": "لَوْ أَنَّ عِندَنَا ذِكْرًا مِّنَ الْأَوَّلِينَ"
			},
			{
				"ayahNumber": "169",
				"ayahText": "لَكُنَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
			},
			{
				"ayahNumber": "170",
				"ayahText": "فَكَفَرُوا بِهِ ۖ فَسَوْفَ يَعْلَمُونَ"
			},
			{
				"ayahNumber": "171",
				"ayahText": "وَلَقَدْ سَبَقَتْ كَلِمَتُنَا لِعِبَادِنَا الْمُرْسَلِينَ"
			},
			{
				"ayahNumber": "172",
				"ayahText": "إِنَّهُمْ لَهُمُ الْمَنصُورُونَ"
			},
			{
				"ayahNumber": "173",
				"ayahText": "وَإِنَّ جُندَنَا لَهُمُ الْغَالِبُونَ"
			},
			{
				"ayahNumber": "174",
				"ayahText": "فَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٍ"
			},
			{
				"ayahNumber": "175",
				"ayahText": "وَأَبْصِرْهُمْ فَسَوْفَ يُبْصِرُونَ"
			},
			{
				"ayahNumber": "176",
				"ayahText": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَ"
			},
			{
				"ayahNumber": "177",
				"ayahText": "فَإِذَا نَزَلَ بِسَاحَتِهِمْ فَسَاءَ صَبَاحُ الْمُنذَرِينَ"
			},
			{
				"ayahNumber": "178",
				"ayahText": "وَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٍ"
			},
			{
				"ayahNumber": "179",
				"ayahText": "وَأَبْصِرْ فَسَوْفَ يُبْصِرُونَ"
			},
			{
				"ayahNumber": "180",
				"ayahText": "سُبْحَانَ رَبِّكَ رَبِّ الْعِزَّةِ عَمَّا يَصِفُونَ"
			},
			{
				"ayahNumber": "181",
				"ayahText": "وَسَلَامٌ عَلَى الْمُرْسَلِينَ"
			},
			{
				"ayahNumber": "182",
				"ayahText": "وَالْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ"
			}
		]
	}
}
