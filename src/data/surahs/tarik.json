{
	"tarik": {
		"surahNumber": "86",
		"surahName": "الطارق",
		"surahNameTr": "Târık",
		"ayats": [
			{
				"ayahNumber": "1",
				"ayahText": "وَالسَّمَاءِ وَالطَّارِقِ"
			},
			{
				"ayahNumber": "2",
				"ayahText": "وَمَا أَدْرَاكَ مَا الطَّارِقُ"
			},
			{
				"ayahNumber": "3",
				"ayahText": "النَّجْمُ الثَّاقِبُ"
			},
			{
				"ayahNumber": "4",
				"ayahText": "إِن كُلُّ نَفْسٍ لَّمَّا عَلَيْهَا حَافِظٌ"
			},
			{
				"ayahNumber": "5",
				"ayahText": "فَلْيَنظُرِ الْإِنسَانُ مِمَّ خُلِقَ"
			},
			{
				"ayahNumber": "6",
				"ayahText": "خُلِقَ مِن مَّاءٍ دَافِقٍ"
			},
			{
				"ayahNumber": "7",
				"ayahText": "يَخْرُجُ مِن بَيْنِ الصُّلْبِ وَالتَّرَائِبِ"
			},
			{
				"ayahNumber": "8",
				"ayahText": "إِنَّهُ عَلَىٰ رَجْعِهِ لَقَادِرٌ"
			},
			{
				"ayahNumber": "9",
				"ayahText": "يَوْمَ تُبْلَى السَّرَائِرُ"
			},
			{
				"ayahNumber": "10",
				"ayahText": "فَمَا لَهُ مِن قُوَّةٍ وَلَا نَاصِرٍ"
			},
			{
				"ayahNumber": "11",
				"ayahText": "وَالسَّمَاءِ ذَاتِ الرَّجْعِ"
			},
			{
				"ayahNumber": "12",
				"ayahText": "وَالْأَرْضِ ذَاتِ الصَّدْعِ"
			},
			{
				"ayahNumber": "13",
				"ayahText": "إِنَّهُ لَقَوْلٌ فَصْلٌ"
			},
			{
				"ayahNumber": "14",
				"ayahText": "وَمَا هُوَ بِالْهَزْلِ"
			},
			{
				"ayahNumber": "15",
				"ayahText": "إِنَّهُمْ يَكِيدُونَ كَيْدًا"
			},
			{
				"ayahNumber": "16",
				"ayahText": "وَأَكِيدُ كَيْدًا"
			},
			{
				"ayahNumber": "17",
				"ayahText": "فَمَهِّلِ الْكَافِرِينَ أَمْهِلْهُمْ رُوَيْدًا"
			}
		]
	}
}
