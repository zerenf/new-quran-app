{
  "ala": {
    "surahNumber": "87",
    "surahName": "الأعلى",
    "surahNameTr": "A'lâ Suresi",
    "ayats": [
      {
        "ayahNumber": "1",
        "ayahText": "سَبِّحِ اسْمَ رَبِّكَ الْأَعْلَى"
      },
      {
        "ayahNumber": "2",
        "ayahText": "الَّذِي خَلَقَ فَسَوَّىٰ"
      },
      {
        "ayahNumber": "3",
        "ayahText": "وَالَّذِي قَدَّرَ فَهَدَىٰ"
      },
      {
        "ayahNumber": "4",
        "ayahText": "وَالَّذِي أَخْرَجَ الْمَرْعَىٰ"
      },
      {
        "ayahNumber": "5",
        "ayahText": "فَجَعَلَهُ غُثَاءً أَحْوَىٰ"
      },
      {
        "ayahNumber": "6",
        "ayahText": "سَنُقْرِئُكَ فَلَا تَنسَىٰ"
      },
      {
        "ayahNumber": "7",
        "ayahText": "إِلَّا مَا شَاءَ اللَّهُ ۚ إِنَّهُ يَعْلَمُ الْجَهْرَ وَمَا يَخْفَىٰ"
      },
      {
        "ayahNumber": "8",
        "ayahText": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ"
      },
      {
        "ayahNumber": "9",
        "ayahText": "فَذَكِّرْ إِن نَّفَعَتِ الذِّكْرَىٰ"
      },
      {
        "ayahNumber": "10",
        "ayahText": "سَيَذَّكَّرُ مَن يَخْشَىٰ"
      },
      {
        "ayahNumber": "11",
        "ayahText": "وَيَتَجَنَّبُهَا الْأَشْقَى"
      },
      {
        "ayahNumber": "12",
        "ayahText": "الَّذِي يَصْلَى النَّارَ الْكُبْرَىٰ"
      },
      {
        "ayahNumber": "13",
        "ayahText": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ"
      },
      {
        "ayahNumber": "14",
        "ayahText": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ"
      },
      {
        "ayahNumber": "15",
        "ayahText": "وَذَكَرَ اسْمَ رَبِّهِ فَصَلَّىٰ"
      },
      {
        "ayahNumber": "16",
        "ayahText": "بَلْ تُؤْثِرُونَ الْحَيَاةَ الدُّنْيَا"
      },
      {
        "ayahNumber": "17",
        "ayahText": "وَالْآخِرَةُ خَيْرٌ وَأَبْقَىٰ"
      },
      {
        "ayahNumber": "18",
        "ayahText": "إِنَّ هَـٰذَا لَفِي الصُّحُفِ الْأُولَىٰ"
      },
      {
        "ayahNumber": "19",
        "ayahText": "صُحُفِ إِبْرَاهِيمَ وَمُوسَىٰ"
      }
    ]
  }
}