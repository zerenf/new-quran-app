	"rahman": {
		"surahNumber": "55",
		"surahName": "الرحمن",
		"surahNameTr": "Rahmân",
		"ayats": [
			{
				"ayahNumber": "1",
				"ayahText": "الرَّحْمَـٰنُ"
			},
			{
				"ayahNumber": "2",
				"ayahText": "عَلَّمَ الْقُرْآنَ"
			},
			{
				"ayahNumber": "3",
				"ayahText": "خَلَقَ الْإِنسَانَ"
			},
			{
				"ayahNumber": "4",
				"ayahText": "عَلَّمَهُ الْبَيَانَ"
			},
			{
				"ayahNumber": "5",
				"ayahText": "الشَّمْسُ وَالْقَمَرُ بِحُسْبَانٍ"
			},
			{
				"ayahNumber": "6",
				"ayahText": "وَالنَّجْمُ وَالشَّجَرُ يَسْجُدَانِ"
			},
			{
				"ayahNumber": "7",
				"ayahText": "وَالسَّمَاءَ رَفَعَهَا وَوَضَعَ الْمِيزَانَ"
			},
			{
				"ayahNumber": "8",
				"ayahText": "أَلَّا تَطْغَوْا فِي الْمِيزَانِ"
			},
			{
				"ayahNumber": "9",
				"ayahText": "وَأَقِيمُوا الْوَزْنَ بِالْقِسْطِ وَلَا تُخْسِرُوا الْمِيزَانَ"
			},
			{
				"ayahNumber": "10",
				"ayahText": "وَالْأَرْضَ وَضَعَهَا لِلْأَنَامِ"
			},
			{
				"ayahNumber": "11",
				"ayahText": "فِيهَا فَاكِهَةٌ وَالنَّخْلُ ذَاتُ الْأَكْمَامِ"
			},
			{
				"ayahNumber": "12",
				"ayahText": "وَالْحَبُّ ذُو الْعَصْفِ وَالرَّيْحَانُ"
			},
			{
				"ayahNumber": "13",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "14",
				"ayahText": "خَلَقَ الْإِنسَانَ مِن صَلْصَالٍ كَالْفَخَّارِ"
			},
			{
				"ayahNumber": "15",
				"ayahText": "وَخَلَقَ الْجَانَّ مِن مَّارِجٍ مِّن نَّارٍ"
			},
			{
				"ayahNumber": "16",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "17",
				"ayahText": "رَبُّ الْمَشْرِقَيْنِ وَرَبُّ الْمَغْرِبَيْنِ"
			},
			{
				"ayahNumber": "18",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "19",
				"ayahText": "مَرَجَ الْبَحْرَيْنِ يَلْتَقِيَانِ"
			},
			{
				"ayahNumber": "20",
				"ayahText": "بَيْنَهُمَا بَرْزَخٌ لَّا يَبْغِيَانِ"
			},
			{
				"ayahNumber": "21",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "22",
				"ayahText": "يَخْرُجُ مِنْهُمَا اللُّؤْلُؤُ وَالْمَرْجَانُ"
			},
			{
				"ayahNumber": "23",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "24",
				"ayahText": "وَلَهُ الْجَوَارِ الْمُنشَآتُ فِي الْبَحْرِ كَالْأَعْلَامِ"
			},
			{
				"ayahNumber": "25",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "26",
				"ayahText": "كُلُّ مَنْ عَلَيْهَا فَانٍ"
			},
			{
				"ayahNumber": "27",
				"ayahText": "وَيَبْقَىٰ وَجْهُ رَبِّكَ ذُو الْجَلَالِ وَالْإِكْرَامِ"
			},
			{
				"ayahNumber": "28",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "29",
				"ayahText": "يَسْأَلُهُ مَن فِي السَّمَاوَاتِ وَالْأَرْضِ ۚ كُلَّ يَوْمٍ هُوَ فِي شَأْنٍ"
			},
			{
				"ayahNumber": "30",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "31",
				"ayahText": "سَنَفْرُغُ لَكُمْ أَيُّهَ الثَّقَلَانِ"
			},
			{
				"ayahNumber": "32",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "33",
				"ayahText": "يَا مَعْشَرَ الْجِنِّ وَالْإِنسِ إِنِ اسْتَطَعْتُمْ أَن تَنفُذُوا مِنْ أَقْطَارِ السَّمَاوَاتِ وَالْأَرْضِ فَانفُذُوا ۚ لَا تَنفُذُونَ إِلَّا بِسُلْطَانٍ"
			},
			{
				"ayahNumber": "34",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "35",
				"ayahText": "يُرْسَلُ عَلَيْكُمَا شُوَاظٌ مِّن نَّارٍ وَنُحَاسٌ فَلَا تَنتَصِرَانِ"
			},
			{
				"ayahNumber": "36",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "37",
				"ayahText": "فَإِذَا انشَقَّتِ السَّمَاءُ فَكَانَتْ وَرْدَةً كَالدِّهَانِ"
			},
			{
				"ayahNumber": "38",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "39",
				"ayahText": "فَيَوْمَئِذٍ لَّا يُسْأَلُ عَن ذَنبِهِ إِنسٌ وَلَا جَانٌّ"
			},
			{
				"ayahNumber": "40",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "41",
				"ayahText": "يُعْرَفُ الْمُجْرِمُونَ بِسِيمَاهُمْ فَيُؤْخَذُ بِالنَّوَاصِي وَالْأَقْدَامِ"
			},
			{
				"ayahNumber": "42",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "43",
				"ayahText": "هَـٰذِهِ جَهَنَّمُ الَّتِي يُكَذِّبُ بِهَا الْمُجْرِمُونَ"
			},
			{
				"ayahNumber": "44",
				"ayahText": "يَطُوفُونَ بَيْنَهَا وَبَيْنَ حَمِيمٍ آنٍ"
			},
			{
				"ayahNumber": "45",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "46",
				"ayahText": "وَلِمَنْ خَافَ مَقَامَ رَبِّهِ جَنَّتَانِ"
			},
			{
				"ayahNumber": "47",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "48",
				"ayahText": "ذَوَاتَا أَفْنَانٍ"
			},
			{
				"ayahNumber": "49",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "50",
				"ayahText": "فِيهِمَا عَيْنَانِ تَجْرِيَانِ"
			},
			{
				"ayahNumber": "51",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "52",
				"ayahText": "فِيهِمَا مِن كُلِّ فَاكِهَةٍ زَوْجَانِ"
			},
			{
				"ayahNumber": "53",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "54",
				"ayahText": "مُتَّكِئِينَ عَلَىٰ فُرُشٍ بَطَائِنُهَا مِنْ إِسْتَبْرَقٍ ۚ وَجَنَى الْجَنَّتَيْنِ دَانٍ"
			},
			{
				"ayahNumber": "55",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "56",
				"ayahText": "فِيهِنَّ قَاصِرَاتُ الطَّرْفِ لَمْ يَطْمِثْهُنَّ إِنسٌ قَبْلَهُمْ وَلَا جَانٌّ"
			},
			{
				"ayahNumber": "57",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "58",
				"ayahText": "كَأَنَّهُنَّ الْيَاقُوتُ وَالْمَرْجَانُ"
			},
			{
				"ayahNumber": "59",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "60",
				"ayahText": "هَلْ جَزَاءُ الْإِحْسَانِ إِلَّا الْإِحْسَانُ"
			},
			{
				"ayahNumber": "61",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "62",
				"ayahText": "وَمِن دُونِهِمَا جَنَّتَانِ"
			},
			{
				"ayahNumber": "63",
				"ayahText": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
			},
			{
				"ayahNumber": "64",
