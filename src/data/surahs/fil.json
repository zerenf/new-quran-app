{
  "fil": {
    "surahNumber": "105",
    "surahName": "الفيل",
    "surahNameTr": "Fîl Suresi",
    "ayats": [
      {
        "ayahNumber": "1",
        "ayahText": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَابِ الْفِيلِ"
      },
      {
        "ayahNumber": "2",
        "ayahText": "أَلَمْ يَجْعَلْ كَيْدَهُمْ فِي تَضْلِيلٍ"
      },
      {
        "ayahNumber": "3",
        "ayahText": "وَأَرْسَلَ عَلَيْهِمْ طَيْرًا أَبَابِيلَ"
      },
      {
        "ayahNumber": "4",
        "ayahText": "تَرْمِيهِم بِحِجَارَةٍ مِّن سِجِّيلٍ"
      },
      {
        "ayahNumber": "5",
        "ayahText": "فَجَعَلَهُمْ كَعَصْفٍ مَّأْكُولٍ"
      }
    ]
  }
}