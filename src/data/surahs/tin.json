{
	"tin": {
		"surahNumber": "95",
		"surahName": "التين",
		"surahNameTr": "Tîn",
		"ayats": [
			{
				"ayahNumber": "1",
				"ayahText": "وَالتِّينِ وَالزَّيْتُونِ"
			},
			{
				"ayahNumber": "2",
				"ayahText": "وَطُورِ سِينِينَ"
			},
			{
				"ayahNumber": "3",
				"ayahText": "وَهَـٰذَا الْبَلَدِ الْأَمِينِ"
			},
			{
				"ayahNumber": "4",
				"ayahText": "لَقَدْ خَلَقْنَا الْإِنسَانَ فِي أَحْسَنِ تَقْوِيمٍ"
			},
			{
				"ayahNumber": "5",
				"ayahText": "ثُمَّ رَدَدْنَاهُ أَسْفَلَ سَافِلِينَ"
			},
			{
				"ayahNumber": "6",
				"ayahText": "إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ فَلَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ"
			},
			{
				"ayahNumber": "7",
				"ayahText": "فَمَا يُكَذِّبُكَ بَعْدُ بِالدِّينِ"
			},
			{
				"ayahNumber": "8",
				"ayahText": "أَلَيْسَ اللَّهُ بِأَحْكَمِ الْحَاكِمِينَ"
			}
		]
	}
}
