{
	"gasiye": {
		"surahNumber": "88",
		"surahName": "الغاشية",
		"surahNameTr": "Ğâşiye",
		"ayats": [
			{
				"ayahNumber": "1",
				"ayahText": "هَلْ أَتَاكَ حَدِيثُ الْغَاشِيَةِ"
			},
			{
				"ayahNumber": "2",
				"ayahText": "وُجُوهٌ يَوْمَئِذٍ خَاشِعَةٌ"
			},
			{
				"ayahNumber": "3",
				"ayahText": "عَامِلَةٌ نَّاصِبَةٌ"
			},
			{
				"ayahNumber": "4",
				"ayahText": "تَصْلَىٰ نَارًا حَامِيَةً"
			},
			{
				"ayahNumber": "5",
				"ayahText": "تُسْقَىٰ مِنْ عَيْنٍ آنِيَةٍ"
			},
			{
				"ayahNumber": "6",
				"ayahText": "لَّيْسَ لَهُمْ طَعَامٌ إِلَّا مِن ضَرِيعٍ"
			},
			{
				"ayahNumber": "7",
				"ayahText": "لَّا يُسْمِنُ وَلَا يُغْنِي مِن جُوعٍ"
			},
			{
				"ayahNumber": "8",
				"ayahText": "وُجُوهٌ يَوْمَئِذٍ نَّاعِمَةٌ"
			},
			{
				"ayahNumber": "9",
				"ayahText": "لِّسَعْيِهَا رَاضِيَةٌ"
			},
			{
				"ayahNumber": "10",
				"ayahText": "فِي جَنَّةٍ عَالِيَةٍ"
			},
			{
				"ayahNumber": "11",
				"ayahText": "لَّا تَسْمَعُ فِيهَا لَاغِيَةً"
			},
			{
				"ayahNumber": "12",
				"ayahText": "فِيهَا عَيْنٌ جَارِيَةٌ"
			},
			{
				"ayahNumber": "13",
				"ayahText": "فِيهَا سُرُرٌ مَّرْفُوعَةٌ"
			},
			{
				"ayahNumber": "14",
				"ayahText": "وَأَكْوَابٌ مَّوْضُوعَةٌ"
			},
			{
				"ayahNumber": "15",
				"ayahText": "وَنَمَارِقُ مَصْفُوفَةٌ"
			},
			{
				"ayahNumber": "16",
				"ayahText": "وَزَرَابِيُّ مَبْثُوثَةٌ"
			},
			{
				"ayahNumber": "17",
				"ayahText": "أَفَلَا يَنظُرُونَ إِلَى الْإِبِلِ كَيْفَ خُلِقَتْ"
			},
			{
				"ayahNumber": "18",
				"ayahText": "وَإِلَى السَّمَاءِ كَيْفَ رُفِعَتْ"
			},
			{
				"ayahNumber": "19",
				"ayahText": "وَإِلَى الْجِبَالِ كَيْفَ نُصِبَتْ"
			},
			{
				"ayahNumber": "20",
				"ayahText": "وَإِلَى الْأَرْضِ كَيْفَ سُطِحَتْ"
			},
			{
				"ayahNumber": "21",
				"ayahText": "فَذَكِّرْ إِنَّمَا أَنتَ مُذَكِّرٌ"
			},
			{
				"ayahNumber": "22",
				"ayahText": "لَّسْتَ عَلَيْهِم بِمُصَيْطِرٍ"
			},
			{
				"ayahNumber": "23",
				"ayahText": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ"
			},
			{
				"ayahNumber": "24",
				"ayahText": "فَيُعَذِّبُهُ اللَّهُ الْعَذَابَ الْأَكْبَرَ"
			},
			{
				"ayahNumber": "25",
				"ayahText": "إِنَّ إِلَيْنَا إِيَابَهُمْ"
			},
			{
				"ayahNumber": "26",
				"ayahText": "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُم"
			}
		]
	}
}
