{
  "fecr": {
    "surahNumber": "89",
    "surahName": "الفجر",
    "surahNameTr": "Fecr Suresi",
    "ayats": [
      {
        "ayahNumber": "1",
        "ayahText": "وَالْفَجْرِ"
      },
      {
        "ayahNumber": "2",
        "ayahText": "وَلَيَالٍ عَشْرٍ"
      },
      {
        "ayahNumber": "3",
        "ayahText": "وَالشَّفْعِ وَالْوَتْرِ"
      },
      {
        "ayahNumber": "4",
        "ayahText": "وَاللَّيْلِ إِذَا يَسْرِ"
      },
      {
        "ayahNumber": "5",
        "ayahText": "هَلْ فِي ذَٰلِكَ قَسَمٌ لِّذِي حِجْرٍ"
      },
      {
        "ayahNumber": "6",
        "ayahText": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ"
      },
      {
        "ayahNumber": "7",
        "ayahText": "إِرَمَ ذَاتِ الْعِمَادِ"
      },
      {
        "ayahNumber": "8",
        "ayahText": "الَّتِي لَمْ يُخْلَقْ مِثْلُهَا فِي الْبِلَادِ"
      },
      {
        "ayahNumber": "9",
        "ayahText": "وَثَمُودَ الَّذِينَ جَابُوا الصَّخْرَ بِالْوَادِ"
      },
      {
        "ayahNumber": "10",
        "ayahText": "وَفِرْعَوْنَ ذِي الْأَوْتَادِ"
      },
      {
        "ayahNumber": "11",
        "ayahText": "الَّذِينَ طَغَوْا فِي الْبِلَادِ"
      },
      {
        "ayahNumber": "12",
        "ayahText": "فَأَكْثَرُوا فِيهَا الْفَسَادَ"
      },
      {
        "ayahNumber": "13",
        "ayahText": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ"
      },
      {
        "ayahNumber": "14",
        "ayahText": "إِنَّ رَبَّكَ لَبِالْمِرْصَادِ"
      },
      {
        "ayahNumber": "15",
        "ayahText": "فَأَمَّا الْإِنسَانُ إِذَا مَا ابْتَلَاهُ رَبُّهُ فَأَكْرَمَهُ وَنَعَّمَهُ فَيَقُولُ رَبِّي أَكْرَمَنِ"
      },
      {
        "ayahNumber": "16",
        "ayahText": "وَأَمَّا إِذَا مَا ابْتَلَاهُ فَقَدَرَ عَلَيْهِ رِزْقَهُ فَيَقُولُ رَبِّي أَهَانَنِ"
      },
      {
        "ayahNumber": "17",
        "ayahText": "كَلَّا ۖ بَل لَّا تُكْرِمُونَ الْيَتِيمَ"
      },
      {
        "ayahNumber": "18",
        "ayahText": "وَلَا تَحَاضُّونَ عَلَىٰ طَعَامِ الْمِسْكِينِ"
      },
      {
        "ayahNumber": "19",
        "ayahText": "وَتَأْكُلُونَ التُّرَاثَ أَكْلًا لَّمًّا"
      },
      {
        "ayahNumber": "20",
        "ayahText": "وَتُحِبُّونَ الْمَالَ حُبًّا جَمًّا"
      },
      {
        "ayahNumber": "21",
        "ayahText": "كَلَّا إِذَا دُكَّتِ الْأَرْضُ دَكًّا دَكًّا"
      },
      {
        "ayahNumber": "22",
        "ayahText": "وَجَاءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّا"
      },
      {
        "ayahNumber": "23",
        "ayahText": "وَجِيءَ يَوْمَئِذٍ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ الْإِنسَانُ وَأَنَّىٰ لَهُ الذِّكْرَىٰ"
      },
      {
        "ayahNumber": "24",
        "ayahText": "يَقُولُ يَا لَيْتَنِي قَدَّمْتُ لِحَيَاتِي"
      },
      {
        "ayahNumber": "25",
        "ayahText": "فَيَوْمَئِذٍ لَّا يُعَذِّبُ عَذَابَهُ أَحَدٌ"
      },
      {
        "ayahNumber": "26",
        "ayahText": "وَلَا يُوثِقُ وَثَاقَهُ أَحَدٌ"
      },
      {
        "ayahNumber": "27",
        "ayahText": "يَا أَيَّتُهَا النَّفْسُ الْمُطْمَئِنَّةُ"
      },
      {
        "ayahNumber": "28",
        "ayahText": "ارْجِعِي إِلَىٰ رَبِّكِ رَاضِيَةً مَّرْضِيَّةً"
      },
      {
        "ayahNumber": "29",
        "ayahText": "فَادْخُلِي فِي عِبَادِي"
      },
      {
        "ayahNumber": "30",
        "ayahText": "وَادْخُلِي جَنَّتِي"
      }
    ]
  }
}