{
	"adiyat": {
		"surahNumber": "100",
		"surahName": "العاديات",
		"surahNameTr": "Adiyât",
		"ayats": [
			{
				"ayahNumber": "1",
				"ayahText": "وَالْعَادِيَاتِ ضَبْحًا"
			},
			{
				"ayahNumber": "2",
				"ayahText": "فَالْمُورِيَاتِ قَدْحًا"
			},
			{
				"ayahNumber": "3",
				"ayahText": "فَالْمُغِيرَاتِ صُبْحًا"
			},
			{
				"ayahNumber": "4",
				"ayahText": "فَأَثَرْنَ بِهِ نَقْعًا"
			},
			{
				"ayahNumber": "5",
				"ayahText": "فَوَسَطْنَ بِهِ جَمْعًا"
			},
			{
				"ayahNumber": "6",
				"ayahText": "إِنَّ الْإِنسَانَ لِرَبِّهِ لَكَنُودٌ"
			},
			{
				"ayahNumber": "7",
				"ayahText": "وَإِنَّهُ عَلَىٰ ذَٰلِكَ لَشَهِيدٌ"
			},
			{
				"ayahNumber": "8",
				"ayahText": "وَإِنَّهُ لِحُبِّ الْخَيْرِ لَشَدِيدٌ"
			},
			{
				"ayahNumber": "9",
				"ayahText": "أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِي الْقُبُورِ"
			},
			{
				"ayahNumber": "10",
				"ayahText": "وَحُصِّلَ مَا فِي الصُّدُورِ"
			},
			{
				"ayahNumber": "11",
				"ayahText": "إِنَّ رَبَّهُم بِهِمْ يَوْمَئِذٍ لَّخَبِيرٌ"
			}
		]
	}
}
