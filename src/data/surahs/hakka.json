{
	"hakka": {
		"surahNumber": "69",
		"surahName": "الحاقة",
		"surahNameTr": "Hâkka",
		"ayats": [
			{
				"ayahNumber": "1",
				"ayahText": "الْحَاقَّةُ"
			},
			{
				"ayahNumber": "2",
				"ayahText": "مَا الْحَاقَّةُ"
			},
			{
				"ayahNumber": "3",
				"ayahText": "وَمَا أَدْرَاكَ مَا الْحَاقَّةُ"
			},
			{
				"ayahNumber": "4",
				"ayahText": "كَذَّبَتْ ثَمُودُ وَعَادٌ بِالْقَارِعَةِ"
			},
			{
				"ayahNumber": "5",
				"ayahText": "فَأَمَّا ثَمُودُ فَأُهْلِكُوا بِالطَّاغِيَةِ"
			},
			{
				"ayahNumber": "6",
				"ayahText": "وَأَمَّا عَادٌ فَأُهْلِكُوا بِرِيحٍ صَرْصَرٍ عَاتِيَةٍ"
			},
			{
				"ayahNumber": "7",
				"ayahText": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَثَمَانِيَةَ أَيَّامٍ حُسُومًا فَتَرَى الْقَوْمَ فِيهَا صَرْعَىٰ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍ"
			},
			{
				"ayahNumber": "8",
				"ayahText": "فَهَلْ تَرَىٰ لَهُم مِّن بَاقِيَةٍ"
			},
			{
				"ayahNumber": "9",
				"ayahText": "وَجَاءَ فِرْعَوْنُ وَمَن قَبْلَهُ وَالْمُؤْتَفِكَاتُ بِالْخَاطِئَةِ"
			},
			{
				"ayahNumber": "10",
				"ayahText": "فَعَصَوْا رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةً رَّابِيَةً"
			},
			{
				"ayahNumber": "11",
				"ayahText": "إِنَّا لَمَّا طَغَى الْمَاءُ حَمَلْنَاكُمْ فِي الْجَارِيَةِ"
			},
			{
				"ayahNumber": "12",
				"ayahText": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَتَعِيَهَا أُذُنٌ وَاعِيَةٌ"
			},
			{
				"ayahNumber": "13",
				"ayahText": "فَإِذَا نُفِخَ فِي الصُّورِ نَفْخَةٌ وَاحِدَةٌ"
			},
			{
				"ayahNumber": "14",
				"ayahText": "وَحُمِلَتِ الْأَرْضُ وَالْجِبَالُ فَدُكَّتَا دَكَّةً وَاحِدَةً"
			},
			{
				"ayahNumber": "15",
				"ayahText": "فَيَوْمَئِذٍ وَقَعَتِ الْوَاقِعَةُ"
			},
			{
				"ayahNumber": "16",
				"ayahText": "وَانشَقَّتِ السَّمَاءُ فَهِيَ يَوْمَئِذٍ وَاهِيَةٌ"
			},
			{
				"ayahNumber": "17",
				"ayahText": "وَالْمَلَكُ عَلَىٰ أَرْجَائِهَا ۚ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍ ثَمَانِيَةٌ"
			},
			{
				"ayahNumber": "18",
				"ayahText": "يَوْمَئِذٍ تُعْرَضُونَ لَا تَخْفَىٰ مِنكُمْ خَافِيَةٌ"
			},
			{
				"ayahNumber": "19",
				"ayahText": "فَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ فَيَقُولُ هَاؤُمُ اقْرَءُوا كِتَابِيَهْ"
			},
			{
				"ayahNumber": "20",
				"ayahText": "إِنِّي ظَنَنتُ أَنِّي مُلَاقٍ حِسَابِيَهْ"
			},
			{
				"ayahNumber": "21",
				"ayahText": "فَهُوَ فِي عِيشَةٍ رَّاضِيَةٍ"
			},
			{
				"ayahNumber": "22",
				"ayahText": "فِي جَنَّةٍ عَالِيَةٍ"
			},
			{
				"ayahNumber": "23",
				"ayahText": "قُطُوفُهَا دَانِيَةٌ"
			},
			{
				"ayahNumber": "24",
				"ayahText": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا أَسْلَفْتُمْ فِي الْأَيَّامِ الْخَالِيَةِ"
			},
			{
				"ayahNumber": "25",
				"ayahText": "وَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِشِمَالِهِ فَيَقُولُ يَا لَيْتَنِي لَمْ أُوتَ كِتَابِيَهْ"
			},
			{
				"ayahNumber": "26",
				"ayahText": "وَلَمْ أَدْرِ مَا حِسَابِيَهْ"
			},
			{
				"ayahNumber": "27",
				"ayahText": "يَا لَيْتَهَا كَانَتِ الْقَاضِيَةَ"
			},
			{
				"ayahNumber": "28",
				"ayahText": "مَا أَغْنَىٰ عَنِّي مَالِيَهْ ۜ"
			},
			{
				"ayahNumber": "29",
				"ayahText": "هَلَكَ عَنِّي سُلْطَانِيَهْ"
			},
			{
				"ayahNumber": "30",
				"ayahText": "خُذُوهُ فَغُلُّوهُ"
			},
			{
				"ayahNumber": "31",
				"ayahText": "ثُمَّ الْجَحِيمَ صَلُّوهُ"
			},
			{
				"ayahNumber": "32",
				"ayahText": "ثُمَّ فِي سِلْسِلَةٍ ذَرْعُهَا سَبْعُونَ ذِرَاعًا فَاسْلُكُوهُ"
			},
			{
				"ayahNumber": "33",
				"ayahText": "إِنَّهُ كَانَ لَا يُؤْمِنُ بِاللَّهِ الْعَظِيمِ"
			},
			{
				"ayahNumber": "34",
				"ayahText": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ الْمِسْكِينِ"
			},
			{
				"ayahNumber": "35",
				"ayahText": "فَلَيْسَ لَهُ الْيَوْمَ هَاهُنَا حَمِيمٌ"
			},
			{
				"ayahNumber": "36",
				"ayahText": "وَلَا طَعَامٌ إِلَّا مِنْ غِسْلِينٍ"
			},
			{
				"ayahNumber": "37",
				"ayahText": "لَّا يَأْكُلُهُ إِلَّا الْخَاطِئُونَ"
			},
			{
				"ayahNumber": "38",
				"ayahText": "فَلَا أُقْسِمُ بِمَا تُبْصِرُونَ"
			},
			{
				"ayahNumber": "39",
				"ayahText": "وَمَا لَا تُبْصِرُونَ"
			},
			{
				"ayahNumber": "40",
				"ayahText": "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ"
			},
			{
				"ayahNumber": "41",
				"ayahText": "وَمَا هُوَ بِقَوْلِ شَاعِرٍ ۚ قَلِيلًا مَّا تُؤْمِنُونَ"
			},
			{
				"ayahNumber": "42",
				"ayahText": "وَلَا بِقَوْلِ كَاهِنٍ ۚ قَلِيلًا مَّا تَذَكَّرُونَ"
			},
			{
				"ayahNumber": "43",
				"ayahText": "تَنزِيلٌ مِّن رَّبِّ الْعَالَمِينَ"
			},
			{
				"ayahNumber": "44",
				"ayahText": "وَلَوْ تَقَوَّلَ عَلَيْنَا بَعْضَ الْأَقَاوِيلِ"
			},
			{
				"ayahNumber": "45",
				"ayahText": "لَأَخَذْنَا مِنْهُ بِالْيَمِينِ"
			},
			{
				"ayahNumber": "46",
				"ayahText": "ثُمَّ لَقَطَعْنَا مِنْهُ الْوَتِينَ"
			},
			{
				"ayahNumber": "47",
				"ayahText": "فَمَا مِنكُم مِّنْ أَحَدٍ عَنْهُ حَاجِزِينَ"
			},
			{
				"ayahNumber": "48",
				"ayahText": "وَإِنَّهُ لَتَذْكِرَةٌ لِّلْمُتَّقِينَ"
			},
			{
				"ayahNumber": "49",
				"ayahText": "وَإِنَّا لَنَعْلَمُ أَنَّ مِنكُم مُّكَذِّبِينَ"
			},
			{
				"ayahNumber": "50",
				"ayahText": "وَإِنَّهُ لَحَسْرَةٌ عَلَى الْكَافِرِينَ"
			},
			{
				"ayahNumber": "51",
				"ayahText": "وَإِنَّهُ لَحَقُّ الْيَقِينِ"
			},
			{
				"ayahNumber": "52",
				"ayahText": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيمِ"
			}
		]
	}
}
