{
  "suara": {
    "surahNumber": "26",
    "surahName": "الشعراء",
    "surahNameTr": "Şuarâ Suresi",
    "ayats": [
      {
        "ayahNumber": "1",
        "ayahText": "طسم"
      },
      {
        "ayahNumber": "2",
        "ayahText": "تِلْكَ آيَاتُ الْكِتَابِ الْمُبِينِ"
      },
      {
        "ayahNumber": "3",
        "ayahText": "لَعَلَّكَ بَاخِعٌ نَّفْسَكَ أَلَّا يَكُونُوا مُؤْمِنِينَ"
      },
      {
        "ayahNumber": "4",
        "ayahText": "إِن نَّشَأْ نُنَزِّلْ عَلَيْهِم مِّنَ السَّمَاءِ آيَةً فَظَلَّتْ أَعْنَاقُهُمْ لَهَا خَاضِعِينَ"
      },
      {
        "ayahNumber": "5",
        "ayahText": "وَمَا يَأْتِيهِم مِّن ذِكْرٍ مِّنَ الرَّحْمَـٰنِ مُحْدَثٍ إِلَّا كَانُوا عَنْهُ مُعْرِضِينَ"
      },
      {
        "ayahNumber": "6",
        "ayahText": "فَقَدْ كَذَّبُوا فَسَيَأْتِيهِمْ أَنبَاءُ مَا كَانُوا بِهِ يَسْتَهْزِئُونَ"
      },
      {
        "ayahNumber": "7",
        "ayahText": "أَوَلَمْ يَرَوْا إِلَى الْأَرْضِ كَمْ أَنبَتْنَا فِيهَا مِن كُلِّ زَوْجٍ كَرِيمٍ"
      },
      {
        "ayahNumber": "8",
        "ayahText": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
      },
      {
        "ayahNumber": "9",
        "ayahText": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
      },
      {
        "ayahNumber": "10",
        "ayahText": "وَإِذْ نَادَىٰ رَبُّكَ مُوسَىٰ أَنِ ائْتِ الْقَوْمَ الظَّالِمِينَ"
      },
      {
        "ayahNumber": "11",
        "ayahText": "قَوْمَ فِرْعَوْنَ ۚ أَلَا يَتَّقُونَ"
      },
      {
        "ayahNumber": "12",
        "ayahText": "قَالَ رَبِّ إِنِّي أَخَافُ أَن يُكَذِّبُونِ"
      },
      {
        "ayahNumber": "13",
        "ayahText": "وَيَضِيقُ صَدْرِي وَلَا يَنطَلِقُ لِسَانِي فَأَرْسِلْ إِلَىٰ هَارُونَ"
      },
      {
        "ayahNumber": "14",
        "ayahText": "وَلَهُمْ عَلَيَّ ذَنبٌ فَأَخَافُ أَن يَقْتُلُونِ"
      },
      {
        "ayahNumber": "15",
        "ayahText": "قَالَ كَلَّا ۖ فَاذْهَبَا بِآيَاتِنَا ۖ إِنَّا مَعَكُم مُّسْتَمِعُونَ"
      },
      {
        "ayahNumber": "16",
        "ayahText": "فَأْتِيَا فِرْعَوْنَ فَقُولَا إِنَّا رَسُولُ رَبِّ الْعَالَمِينَ"
      },
      {
        "ayahNumber": "17",
        "ayahText": "أَنْ أَرْسِلْ مَعَنَا بَنِي إِسْرَائِيلَ"
      },
      {
        "ayahNumber": "18",
        "ayahText": "قَالَ أَلَمْ نُرَبِّكَ فِينَا وَلِيدًا وَلَبِثْتَ فِينَا مِنْ عُمُرِكَ سِنِينَ"
      },
      {
        "ayahNumber": "19",
        "ayahText": "وَفَعَلْتَ فَعْلَتَكَ الَّتِي فَعَلْتَ وَأَنتَ مِنَ الْكَافِرِينَ"
      },
      {
        "ayahNumber": "20",
        "ayahText": "قَالَ فَعَلْتُهَا إِذًا وَأَنَا مِنَ الضَّالِّينَ"
      },
      {
        "ayahNumber": "21",
        "ayahText": "فَفَرَرْتُ مِنكُمْ لَمَّا خِفْتُكُمْ فَوَهَبَ لِي رَبِّي حُكْمًا وَجَعَلَنِي مِنَ الْمُرْسَلِينَ"
      },
      {
        "ayahNumber": "22",
        "ayahText": "وَتِلْكَ نِعْمَةٌ تَمُنُّهَا عَلَيَّ أَنْ عَبَّدتَّ بَنِي إِسْرَائِيلَ"
      },
      {
        "ayahNumber": "23",
        "ayahText": "قَالَ فِرْعَوْنُ وَمَا رَبُّ الْعَالَمِينَ"
      },
      {
        "ayahNumber": "24",
        "ayahText": "قَالَ رَبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا ۖ إِن كُنتُم مُّوقِنِينَ"
      },
      {
        "ayahNumber": "25",
        "ayahText": "قَالَ لِمَنْ حَوْلَهُ أَلَا تَسْتَمِعُونَ"
      },
      {
        "ayahNumber": "26",
        "ayahText": "قَالَ رَبُّكُمْ وَرَبُّ آبَائِكُمُ الْأَوَّلِينَ"
      },
      {
        "ayahNumber": "27",
        "ayahText": "قَالَ إِنَّ رَسُولَكُمُ الَّذِي أُرْسِلَ إِلَيْكُمْ لَمَجْنُونٌ"
      },
      {
        "ayahNumber": "28",
        "ayahText": "قَالَ رَبُّ الْمَشْرِقِ وَالْمَغْرِبِ وَمَا بَيْنَهُمَا ۖ إِن كُنتُمْ تَعْقِلُونَ"
      },
      {
        "ayahNumber": "29",
        "ayahText": "قَالَ لَئِنِ اتَّخَذْتَ إِلَـٰهًا غَيْرِي لَأَجْعَلَنَّكَ مِنَ الْمَسْجُونِينَ"
      },
      {
        "ayahNumber": "30",
        "ayahText": "قَالَ أَوَلَوْ جِئْتُكَ بِشَيْءٍ مُّبِينٍ"
      },
      {
        "ayahNumber": "31",
        "ayahText": "قَالَ فَأْتِ بِهِ إِن كُنتَ مِنَ الصَّادِقِينَ"
      },
      {
        "ayahNumber": "32",
        "ayahText": "فَأَلْقَىٰ عَصَاهُ فَإِذَا هِيَ ثُعْبَانٌ مُّبِينٌ"
      },
      {
        "ayahNumber": "33",
        "ayahText": "وَنَزَعَ يَدَهُ فَإِذَا هِيَ بَيْضَاءُ لِلنَّاظِرِينَ"
      },
      {
        "ayahNumber": "34",
        "ayahText": "قَالَ لِلْمَلَإِ حَوْلَهُ إِنَّ هَـٰذَا لَسَاحِرٌ عَلِيمٌ"
      },
      {
        "ayahNumber": "35",
        "ayahText": "يُرِيدُ أَن يُخْرِجَكُم مِّنْ أَرْضِكُم بِسِحْرِهِ فَمَاذَا تَأْمُرُونَ"
      },
      {
        "ayahNumber": "36",
        "ayahText": "قَالُوا أَرْجِهْ وَأَخَاهُ وَابْعَثْ فِي الْمَدَائِنِ حَاشِرِينَ"
      },
      {
        "ayahNumber": "37",
        "ayahText": "يَأْتُوكَ بِكُلِّ سَحَّارٍ عَلِيمٍ"
      },
      {
        "ayahNumber": "38",
        "ayahText": "فَجُمِعَ السَّحَرَةُ لِمِيقَاتِ يَوْمٍ مَّعْلُومٍ"
      },
      {
        "ayahNumber": "39",
        "ayahText": "وَقِيلَ لِلنَّاسِ هَلْ أَنتُم مُّجْتَمِعُونَ"
      },
      {
        "ayahNumber": "40",
        "ayahText": "لَعَلَّنَا نَتَّبِعُ السَّحَرَةَ إِن كَانُوا هُمُ الْغَالِبِينَ"
      },
      {
        "ayahNumber": "41",
        "ayahText": "فَلَمَّا جَاءَ السَّحَرَةُ قَالُوا لِفِرْعَوْنَ أَئِنَّ لَنَا لَأَجْرًا إِن كُنَّا نَحْنُ الْغَالِبِينَ"
      },
      {
        "ayahNumber": "42",
        "ayahText": "قَالَ نَعَمْ وَإِنَّكُمْ إِذًا لَّمِنَ الْمُقَرَّبِينَ"
      },
      {
        "ayahNumber": "43",
        "ayahText": "قَالَ لَهُم مُّوسَىٰ أَلْقُوا مَا أَنتُم مُّلْقُونَ"
      },
      {
        "ayahNumber": "44",
        "ayahText": "فَأَلْقَوْا حِبَالَهُمْ وَعِصِيَّهُمْ وَقَالُوا بِعِزَّةِ فِرْعَوْنَ إِنَّا لَنَحْنُ الْغَالِبُونَ"
      },
      {
        "ayahNumber": "45",
        "ayahText": "فَأَلْقَىٰ مُوسَىٰ عَصَاهُ فَإِذَا هِيَ تَلْقَفُ مَا يَأْفِكُونَ"
      },
      {
        "ayahNumber": "46",
        "ayahText": "فَأُلْقِيَ السَّحَرَةُ سَاجِدِينَ"
      },
      {
        "ayahNumber": "47",
        "ayahText": "قَالُوا آمَنَّا بِرَبِّ الْعَالَمِينَ"
      },
      {
        "ayahNumber": "48",
        "ayahText": "رَبِّ مُوسَىٰ وَهَارُونَ"
      },
      {
        "ayahNumber": "49",
        "ayahText": "قَالَ آمَنتُمْ لَهُ قَبْلَ أَنْ آذَنَ لَكُمْ ۖ إِنَّهُ لَكَبِيرُكُمُ الَّذِي عَلَّمَكُمُ السِّحْرَ فَلَسَوْفَ تَعْلَمُونَ ۚ لَأُقَطِّعَنَّ أَيْدِيَكُمْ وَأَرْجُلَكُم مِّنْ خِلَافٍ وَلَأُصَلِّبَنَّكُمْ أَجْمَعِينَ"
      },
      {
        "ayahNumber": "50",
        "ayahText": "قَالُوا لَا ضَيْرَ ۖ إِنَّا إِلَىٰ رَبِّنَا مُنقَلِبُونَ"
      },
      {
        "ayahNumber": "51",
        "ayahText": "إِنَّا نَطْمَعُ أَن يَغْفِرَ لَنَا رَبُّنَا خَطَايَانَا أَن كُنَّا أَوَّلَ الْمُؤْمِنِينَ"
      },
      {
        "ayahNumber": "52",
        "ayahText": "وَأَوْحَيْنَا إِلَىٰ مُوسَىٰ أَنْ أَسْرِ بِعِبَادِي إِنَّكُم مُّتَّبَعُونَ"
      },
      {
        "ayahNumber": "53",
        "ayahText": "فَأَرْسَلَ فِرْعَوْنُ فِي الْمَدَائِنِ حَاشِرِينَ"
      },
      {
        "ayahNumber": "54",
        "ayahText": "إِنَّ هَـٰؤُلَاءِ لَشِرْذِمَةٌ قَلِيلُونَ"
      },
      {
        "ayahNumber": "55",
        "ayahText": "وَإِنَّهُمْ لَنَا لَغَائِظُونَ"
      },
      {
        "ayahNumber": "56",
        "ayahText": "وَإِنَّا لَجَمِيعٌ حَاذِرُونَ"
      },
      {
        "ayahNumber": "57",
        "ayahText": "فَأَخْرَجْنَاهُم مِّن جَنَّاتٍ وَعُيُونٍ"
      },
      {
        "ayahNumber": "58",
        "ayahText": "وَكُنُوزٍ وَمَقَامٍ كَرِيمٍ"
      },
      {
        "ayahNumber": "59",
        "ayahText": "كَذَٰلِكَ وَأَوْرَثْنَاهَا بَنِي إِسْرَائِيلَ"
      },
      {
        "ayahNumber": "60",
        "ayahText": "فَأَتْبَعُوهُم مُّشْرِقِينَ"
      },
      {
        "ayahNumber": "61",
        "ayahText": "فَلَمَّا تَرَاءَى الْجَمْعَانِ قَالَ أَصْحَابُ مُوسَىٰ إِنَّا لَمُدْرَكُونَ"
      },
      {
        "ayahNumber": "62",
        "ayahText": "قَالَ كَلَّا ۖ إِنَّ مَعِيَ رَبِّي سَيَهْدِينِ"
      },
      {
        "ayahNumber": "63",
        "ayahText": "فَأَوْحَيْنَا إِلَىٰ مُوسَىٰ أَنِ اضْرِب بِّعَصَاكَ الْبَحْرَ ۖ فَانفَلَقَ فَكَانَ كُلُّ فِرْقٍ كَالطَّوْدِ الْعَظِيمِ"
      },
      {
        "ayahNumber": "64",
        "ayahText": "وَأَزْلَفْنَا ثَمَّ الْآخَرِينَ"
      },
      {
        "ayahNumber": "65",
        "ayahText": "وَأَنجَيْنَا مُوسَىٰ وَمَن مَّعَهُ أَجْمَعِينَ"
      },
      {
        "ayahNumber": "66",
        "ayahText": "ثُمَّ أَغْرَقْنَا الْآخَرِينَ"
      },
      {
        "ayahNumber": "67",
        "ayahText": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
      },
      {
        "ayahNumber": "68",
        "ayahText": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
      },
      {
        "ayahNumber": "69",
        "ayahText": "وَاتْلُ عَلَيْهِمْ نَبَأَ إِبْرَاهِيمَ"
      },
      {
        "ayahNumber": "70",
        "ayahText": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِ مَا تَعْبُدُونَ"
      },
      {
        "ayahNumber": "71",
        "ayahText": "قَالُوا نَعْبُدُ أَصْنَامًا فَنَظَلُّ لَهَا عَاكِفِينَ"
      },
      {
        "ayahNumber": "72",
        "ayahText": "قَالَ هَلْ يَسْمَعُونَكُمْ إِذْ تَدْعُونَ"
      },
      {
        "ayahNumber": "73",
        "ayahText": "أَوْ يَنفَعُونَكُمْ أَوْ يَضُرُّونَ"
      },
      {
        "ayahNumber": "74",
        "ayahText": "قَالُوا بَلْ وَجَدْنَا آبَاءَنَا كَذَٰلِكَ يَفْعَلُونَ"
      },
      {
        "ayahNumber": "75",
        "ayahText": "قَالَ أَفَرَأَيْتُم مَّا كُنتُمْ تَعْبُدُونَ"
      },
      {
        "ayahNumber": "76",
        "ayahText": "أَنتُمْ وَآبَاؤُكُمُ الْأَقْدَمُونَ"
      },
      {
        "ayahNumber": "77",
        "ayahText": "فَإِنَّهُمْ عَدُوٌّ لِّي إِلَّا رَبَّ الْعَالَمِينَ"
      },
      {
        "ayahNumber": "78",
        "ayahText": "الَّذِي خَلَقَنِي فَهُوَ يَهْدِينِ"
      },
      {
        "ayahNumber": "79",
        "ayahText": "وَالَّذِي هُوَ يُطْعِمُنِي وَيَسْقِينِ"
      },
      {
        "ayahNumber": "80",
        "ayahText": "وَإِذَا مَرِضْتُ فَهُوَ يَشْفِينِ"
      },
      {
        "ayahNumber": "81",
        "ayahText": "وَالَّذِي يُمِيتُنِي ثُمَّ يُحْيِينِ"
      },
      {
        "ayahNumber": "82",
        "ayahText": "وَالَّذِي أَطْمَعُ أَن يَغْفِرَ لِي خَطِيئَتِي يَوْمَ الدِّينِ"
      },
      {
        "ayahNumber": "83",
        "ayahText": "رَبِّ هَبْ لِي حُكْمًا وَأَلْحِقْنِي بِالصَّالِحِينَ"
      },
      {
        "ayahNumber": "84",
        "ayahText": "وَاجْعَل لِّي لِسَانَ صِدْقٍ فِي الْآخِرِينَ"
      },
      {
        "ayahNumber": "85",
        "ayahText": "وَاجْعَلْنِي مِن وَرَثَةِ جَنَّةِ النَّعِيمِ"
      },
      {
        "ayahNumber": "86",
        "ayahText": "وَاغْفِرْ لِأَبِي إِنَّهُ كَانَ مِنَ الضَّالِّينَ"
      },
      {
        "ayahNumber": "87",
        "ayahText": "وَلَا تُخْزِنِي يَوْمَ يُبْعَثُونَ"
      },
      {
        "ayahNumber": "88",
        "ayahText": "يَوْمَ لَا يَنفَعُ مَالٌ وَلَا بَنُونَ"
      },
      {
        "ayahNumber": "89",
        "ayahText": "إِلَّا مَنْ أَتَى اللَّهَ بِقَلْبٍ سَلِيمٍ"
      },
      {
        "ayahNumber": "90",
        "ayahText": "وَأُزْلِفَتِ الْجَنَّةُ لِلْمُتَّقِينَ"
      },
      {
        "ayahNumber": "91",
        "ayahText": "وَبُرِّزَتِ الْجَحِيمُ لِلْغَاوِينَ"
      },
      {
        "ayahNumber": "92",
        "ayahText": "وَقِيلَ لَهُمْ أَيْنَ مَا كُنتُمْ تَعْبُدُونَ"
      },
      {
        "ayahNumber": "93",
        "ayahText": "مِن دُونِ اللَّهِ هَلْ يَنصُرُونَكُمْ أَوْ يَنتَصِرُونَ"
      },
      {
        "ayahNumber": "94",
        "ayahText": "فَكُبْكِبُوا فِيهَا هُمْ وَالْغَاوُونَ"
      },
      {
        "ayahNumber": "95",
        "ayahText": "وَجُنُودُ إِبْلِيسَ أَجْمَعُونَ"
      },
      {
        "ayahNumber": "96",
        "ayahText": "قَالُوا وَهُمْ فِيهَا يَخْتَصِمُونَ"
      },
      {
        "ayahNumber": "97",
        "ayahText": "تَاللَّهِ إِن كُنَّا لَفِي ضَلَالٍ مُّبِينٍ"
      },
      {
        "ayahNumber": "98",
        "ayahText": "إِذْ نُسَوِّيكُم بِرَبِّ الْعَالَمِينَ"
      },
      {
        "ayahNumber": "99",
        "ayahText": "وَمَا أَضَلَّنَا إِلَّا الْمُجْرِمُونَ"
      },
      {
        "ayahNumber": "100",
        "ayahText": "فَمَا لَنَا مِن شَافِعِينَ"
      },
      {
        "ayahNumber": "101",
        "ayahText": "وَلَا صَدِيقٍ حَمِيمٍ"
      },
      {
        "ayahNumber": "102",
        "ayahText": "فَلَوْ أَنَّ لَنَا كَرَّةً فَنَكُونَ مِنَ الْمُؤْمِنِينَ"
      },
      {
        "ayahNumber": "103",
        "ayahText": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
      },
      {
        "ayahNumber": "104",
        "ayahText": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
      },
      {
        "ayahNumber": "105",
        "ayahText": "كَذَّبَتْ قَوْمُ نُوحٍ الْمُرْسَلِينَ"
      },
      {
        "ayahNumber": "106",
        "ayahText": "إِذْ قَالَ لَهُمْ أَخُوهُمْ نُوحٌ أَلَا تَتَّقُونَ"
      },
      {
        "ayahNumber": "107",
        "ayahText": "إِنِّي لَكُمْ رَسُولٌ أَمِينٌ"
      },
      {
        "ayahNumber": "108",
        "ayahText": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
      },
      {
        "ayahNumber": "109",
        "ayahText": "وَمَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِيَ إِلَّا عَلَىٰ رَبِّ الْعَالَمِينَ"
      },
      {
        "ayahNumber": "110",
        "ayahText": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
      },
      {
        "ayahNumber": "111",
        "ayahText": "قَالُوا أَنُؤْمِنُ لَكَ وَاتَّبَعَكَ الْأَرْذَلُونَ"
      },
      {
        "ayahNumber": "112",
        "ayahText": "قَالَ وَمَا عِلْمِي بِمَا كَانُوا يَعْمَلُونَ"
      },
      {
        "ayahNumber": "113",
        "ayahText": "إِنْ حِسَابُهُمْ إِلَّا عَلَىٰ رَبِّي ۖ لَوْ تَشْعُرُونَ"
      },
      {
        "ayahNumber": "114",
        "ayahText": "وَمَا أَنَا بِطَارِدِ الْمُؤْمِنِينَ"
      },
      {
        "ayahNumber": "115",
        "ayahText": "إِنْ أَنَا إِلَّا نَذِيرٌ مُّبِينٌ"
      },
      {
        "ayahNumber": "116",
        "ayahText": "قَالُوا لَئِن لَّمْ تَنتَهِ يَا نُوحُ لَتَكُونَنَّ مِنَ الْمَرْجُومِينَ"
      },
      {
        "ayahNumber": "117",
        "ayahText": "قَالَ رَبِّ إِنَّ قَوْمِي كَذَّبُونِ"
      },
      {
        "ayahNumber": "118",
        "ayahText": "فَافْتَحْ بَيْنِي وَبَيْنَهُمْ فَتْحًا وَنَجِّنِي وَمَن مَّعِيَ مِنَ الْمُؤْمِنِينَ"
      },
      {
        "ayahNumber": "119",
        "ayahText": "فَأَنجَيْنَاهُ وَمَن مَّعَهُ فِي الْفُلْكِ الْمَشْحُونِ"
      },
      {
        "ayahNumber": "120",
        "ayahText": "ثُمَّ أَغْرَقْنَا بَعْدُ الْبَاقِينَ"
      },
      {
        "ayahNumber": "121",
        "ayahText": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
      },
      {
        "ayahNumber": "122",
        "ayahText": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
      },
      {
        "ayahNumber": "123",
        "ayahText": "كَذَّبَتْ عَادٌ الْمُرْسَلِينَ"
      },
      {
        "ayahNumber": "124",
        "ayahText": "إِذْ قَالَ لَهُمْ أَخُوهُمْ هُودٌ أَلَا تَتَّقُونَ"
      },
      {
        "ayahNumber": "125",
        "ayahText": "إِنِّي لَكُمْ رَسُولٌ أَمِينٌ"
      },
      {
        "ayahNumber": "126",
        "ayahText": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
      },
      {
        "ayahNumber": "127",
        "ayahText": "وَمَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِيَ إِلَّا عَلَىٰ رَبِّ الْعَالَمِينَ"
      },
      {
        "ayahNumber": "128",
        "ayahText": "أَتَبْنُونَ بِكُلِّ رِيعٍ آيَةً تَعْبَثُونَ"
      },
      {
        "ayahNumber": "129",
        "ayahText": "وَتَتَّخِذُونَ مَصَانِعَ لَعَلَّكُمْ تَخْلُدُونَ"
      },
      {
        "ayahNumber": "130",
        "ayahText": "وَإِذَا بَطَشْتُم بَطَشْتُمْ جَبَّارِينَ"
      },
      {
        "ayahNumber": "131",
        "ayahText": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
      },
      {
        "ayahNumber": "132",
        "ayahText": "وَاتَّقُوا الَّذِي أَمَدَّكُم بِمَا تَعْلَمُونَ"
      },
      {
        "ayahNumber": "133",
        "ayahText": "أَمَدَّكُم بِأَنْعَامٍ وَبَنِينَ"
      },
      {
        "ayahNumber": "134",
        "ayahText": "وَجَنَّاتٍ وَعُيُونٍ"
      },
      {
        "ayahNumber": "135",
        "ayahText": "إِنِّي أَخَافُ عَلَيْكُمْ عَذَابَ يَوْمٍ عَظِيمٍ"
      },
      {
        "ayahNumber": "136",
        "ayahText": "قَالُوا سَوَاءٌ عَلَيْنَا أَوَعَظْتَ أَمْ لَمْ تَكُن مِّنَ الْوَاعِظِينَ"
      },
      {
        "ayahNumber": "137",
        "ayahText": "إِنْ هَـٰذَا إِلَّا خُلُقُ الْأَوَّلِينَ"
      },
      {
        "ayahNumber": "138",
        "ayahText": "وَمَا نَحْنُ بِمُعَذَّبِينَ"
      },
      {
        "ayahNumber": "139",
        "ayahText": "فَكَذَّبُوهُ فَأَهْلَكْنَاهُمْ ۗ إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
      },
      {
        "ayahNumber": "140",
        "ayahText": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
      },
      {
        "ayahNumber": "141",
        "ayahText": "كَذَّبَتْ ثَمُودُ الْمُرْسَلِينَ"
      },
      {
        "ayahNumber": "142",
        "ayahText": "إِذْ قَالَ لَهُمْ أَخُوهُمْ صَالِحٌ أَلَا تَتَّقُونَ"
      },
      {
        "ayahNumber": "143",
        "ayahText": "إِنِّي لَكُمْ رَسُولٌ أَمِينٌ"
      },
      {
        "ayahNumber": "144",
        "ayahText": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
      },
      {
        "ayahNumber": "145",
        "ayahText": "وَمَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِيَ إِلَّا عَلَىٰ رَبِّ الْعَالَمِينَ"
      },
      {
        "ayahNumber": "146",
        "ayahText": "أَتُتْرَكُونَ فِي مَا هَاهُنَا آمِنِينَ"
      },
      {
        "ayahNumber": "147",
        "ayahText": "فِي جَنَّاتٍ وَعُيُونٍ"
      },
      {
        "ayahNumber": "148",
        "ayahText": "وَزُرُوعٍ وَنَخْلٍ طَلْعُهَا هَضِيمٌ"
      },
      {
        "ayahNumber": "149",
        "ayahText": "وَتَنْحِتُونَ مِنَ الْجِبَالِ بُيُوتًا فَارِهِينَ"
      },
      {
        "ayahNumber": "150",
        "ayahText": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
      },
      {
        "ayahNumber": "151",
        "ayahText": "وَلَا تُطِيعُوا أَمْرَ الْمُسْرِفِينَ"
      },
      {
        "ayahNumber": "152",
        "ayahText": "الَّذِينَ يُفْسِدُونَ فِي الْأَرْضِ وَلَا يُصْلِحُونَ"
      },
      {
        "ayahNumber": "153",
        "ayahText": "قَالُوا إِنَّمَا أَنتَ مِنَ الْمُسَحَّرِينَ"
      },
      {
        "ayahNumber": "154",
        "ayahText": "مَا أَنتَ إِلَّا بَشَرٌ مِّثْلُنَا فَأْتِ بِآيَةٍ إِن كُنتَ مِنَ الصَّادِقِينَ"
      },
      {
        "ayahNumber": "155",
        "ayahText": "قَالَ هَـٰذِهِ نَاقَةٌ لَّهَا شِرْبٌ وَلَكُمْ شِرْبُ يَوْمٍ مَّعْلُومٍ"
      },
      {
        "ayahNumber": "156",
        "ayahText": "وَلَا تَمَسُّوهَا بِسُوءٍ فَيَأْخُذَكُمْ عَذَابُ يَوْمٍ عَظِيمٍ"
      },
      {
        "ayahNumber": "157",
        "ayahText": "فَعَقَرُوهَا فَأَصْبَحُوا نَادِمِينَ"
      },
      {
        "ayahNumber": "158",
        "ayahText": "فَأَخَذَهُمُ الْعَذَابُ ۗ إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
      },
      {
        "ayahNumber": "159",
        "ayahText": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
      },
      {
        "ayahNumber": "160",
        "ayahText": "كَذَّبَتْ قَوْمُ لُوطٍ الْمُرْسَلِينَ"
      },
      {
        "ayahNumber": "161",
        "ayahText": "إِذْ قَالَ لَهُمْ أَخُوهُمْ لُوطٌ أَلَا تَتَّقُونَ"
      },
      {
        "ayahNumber": "162",
        "ayahText": "إِنِّي لَكُمْ رَسُولٌ أَمِينٌ"
      },
      {
        "ayahNumber": "163",
        "ayahText": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
      },
      {
        "ayahNumber": "164",
        "ayahText": "وَمَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِيَ إِلَّا عَلَىٰ رَبِّ الْعَالَمِينَ"
      },
      {
        "ayahNumber": "165",
        "ayahText": "أَتَأْتُونَ الذُّكْرَانَ مِنَ الْعَالَمِينَ"
      },
      {
        "ayahNumber": "166",
        "ayahText": "وَتَذَرُونَ مَا خَلَقَ لَكُمْ رَبُّكُم مِّنْ أَزْوَاجِكُم ۚ بَلْ أَنتُمْ قَوْمٌ عَادُونَ"
      },
      {
        "ayahNumber": "167",
        "ayahText": "قَالُوا لَئِن لَّمْ تَنتَهِ يَا لُوطُ لَتَكُونَنَّ مِنَ الْمُخْرَجِينَ"
      },
      {
        "ayahNumber": "168",
        "ayahText": "قَالَ إِنِّي لِعَمَلِكُم مِّنَ الْقَالِينَ"
      },
      {
        "ayahNumber": "169",
        "ayahText": "رَبِّ نَجِّنِي وَأَهْلِي مِمَّا يَعْمَلُونَ"
      },
      {
        "ayahNumber": "170",
        "ayahText": "فَنَجَّيْنَاهُ وَأَهْلَهُ أَجْمَعِينَ"
      },
      {
        "ayahNumber": "171",
        "ayahText": "إِلَّا عَجُوزًا فِي الْغَابِرِينَ"
      },
      {
        "ayahNumber": "172",
        "ayahText": "ثُمَّ دَمَّرْنَا الْآخَرِينَ"
      },
      {
        "ayahNumber": "173",
        "ayahText": "وَأَمْطَرْنَا عَلَيْهِم مَّطَرًا ۖ فَسَاءَ مَطَرُ الْمُنذَرِينَ"
      },
      {
        "ayahNumber": "174",
        "ayahText": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
      },
      {
        "ayahNumber": "175",
        "ayahText": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
      },
      {
        "ayahNumber": "176",
        "ayahText": "كَذَّبَ أَصْحَابُ الْأَيْكَةِ الْمُرْسَلِينَ"
      },
      {
        "ayahNumber": "177",
        "ayahText": "إِذْ قَالَ لَهُمْ شُعَيْبٌ أَلَا تَتَّقُونَ"
      },
      {
        "ayahNumber": "178",
        "ayahText": "إِنِّي لَكُمْ رَسُولٌ أَمِينٌ"
      },
      {
        "ayahNumber": "179",
        "ayahText": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
      },
      {
        "ayahNumber": "180",
        "ayahText": "وَمَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِيَ إِلَّا عَلَىٰ رَبِّ الْعَالَمِينَ"
      },
      {
        "ayahNumber": "181",
        "ayahText": "أَوْفُوا الْكَيْلَ وَلَا تَكُونُوا مِنَ الْمُخْسِرِينَ"
      },
      {
        "ayahNumber": "182",
        "ayahText": "وَزِنُوا بِالْقِسْطَاسِ الْمُسْتَقِيمِ"
      },
      {
        "ayahNumber": "183",
        "ayahText": "وَلَا تَبْخَسُوا النَّاسَ أَشْيَاءَهُمْ وَلَا تَعْثَوْا فِي الْأَرْضِ مُفْسِدِينَ"
      },
      {
        "ayahNumber": "184",
        "ayahText": "وَاتَّقُوا الَّذِي خَلَقَكُمْ وَالْجِبِلَّةَ الْأَوَّلِينَ"
      },
      {
        "ayahNumber": "185",
        "ayahText": "قَالُوا إِنَّمَا أَنتَ مِنَ الْمُسَحَّرِينَ"
      },
      {
        "ayahNumber": "186",
        "ayahText": "وَمَا أَنتَ إِلَّا بَشَرٌ مِّثْلُنَا وَإِن نَّظُنُّكَ لَمِنَ الْكَاذِبِينَ"
      },
      {
        "ayahNumber": "187",
        "ayahText": "فَأَسْقِطْ عَلَيْنَا كِسَفًا مِّنَ السَّمَاءِ إِن كُنتَ مِنَ الصَّادِقِينَ"
      },
      {
        "ayahNumber": "188",
        "ayahText": "قَالَ رَبِّي أَعْلَمُ بِمَا تَعْمَلُونَ"
      },
      {
        "ayahNumber": "189",
        "ayahText": "فَكَذَّبُوهُ فَأَخَذَهُمْ عَذَابُ يَوْمِ الظُّلَّةِ ۚ إِنَّهُ كَانَ عَذَابَ يَوْمٍ عَظِيمٍ"
      },
      {
        "ayahNumber": "190",
        "ayahText": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
      },
      {
        "ayahNumber": "191",
        "ayahText": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
      },
      {
        "ayahNumber": "192",
        "ayahText": "وَإِنَّهُ لَتَنزِيلُ رَبِّ الْعَالَمِينَ"
      },
      {
        "ayahNumber": "193",
        "ayahText": "نَزَلَ بِهِ الرُّوحُ الْأَمِينُ"
      },
      {
        "ayahNumber": "194",
        "ayahText": "عَلَىٰ قَلْبِكَ لِتَكُونَ مِنَ الْمُنذِرِينَ"
      },
      {
        "ayahNumber": "195",
        "ayahText": "بِلِسَانٍ عَرَبِيٍّ مُّبِينٍ"
      },
      {
        "ayahNumber": "196",
        "ayahText": "وَإِنَّهُ لَفِي زُبُرِ الْأَوَّلِينَ"
      },
      {
        "ayahNumber": "197",
        "ayahText": "أَوَلَمْ يَكُن لَّهُمْ آيَةً أَن يَعْلَمَهُ عُلَمَاءُ بَنِي إِسْرَائِيلَ"
      },
      {
        "ayahNumber": "198",
        "ayahText": "وَلَوْ نَزَّلْنَاهُ عَلَىٰ بَعْضِ الْأَعْجَمِينَ"
      },
      {
        "ayahNumber": "199",
        "ayahText": "فَقَرَأَهُ عَلَيْهِم مَّا كَانُوا بِهِ مُؤْمِنِينَ"
      },
      {
        "ayahNumber": "200",
        "ayahText": "كَذَٰلِكَ سَلَكْنَاهُ فِي قُلُوبِ الْمُجْرِمِينَ"
      },
      {
        "ayahNumber": "201",
        "ayahText": "لَا يُؤْمِنُونَ بِهِ حَتَّىٰ يَرَوُا الْعَذَابَ الْأَلِيمَ"
      },
      {
        "ayahNumber": "202",
        "ayahText": "فَيَأْتِيَهُم بَغْتَةً وَهُمْ لَا يَشْعُرُونَ"
      },
      {
        "ayahNumber": "203",
        "ayahText": "فَيَقُولُوا هَلْ نَحْنُ مُنظَرُونَ"
      },
      {
        "ayahNumber": "204",
        "ayahText": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَ"
      },
      {
        "ayahNumber": "205",
        "ayahText": "أَفَرَأَيْتَ إِن مَّتَّعْنَاهُمْ سِنِينَ"
      },
      {
        "ayahNumber": "206",
        "ayahText": "ثُمَّ جَاءَهُم مَّا كَانُوا يُوعَدُونَ"
      },
      {
        "ayahNumber": "207",
        "ayahText": "مَا أَغْنَىٰ عَنْهُم مَّا كَانُوا يُمَتَّعُونَ"
      },
      {
        "ayahNumber": "208",
        "ayahText": "وَمَا أَهْلَكْنَا مِن قَرْيَةٍ إِلَّا لَهَا مُنذِرُونَ"
      },
      {
        "ayahNumber": "209",
        "ayahText": "ذِكْرَىٰ وَمَا كُنَّا ظَالِمِينَ"
      },
      {
        "ayahNumber": "210",
        "ayahText": "وَمَا تَنَزَّلَتْ بِهِ الشَّيَاطِينُ"
      },
      {
        "ayahNumber": "211",
        "ayahText": "وَمَا يَنبَغِي لَهُمْ وَمَا يَسْتَطِيعُونَ"
      },
      {
        "ayahNumber": "212",
        "ayahText": "إِنَّهُمْ عَنِ السَّمْعِ لَمَعْزُولُونَ"
      },
      {
        "ayahNumber": "213",
        "ayahText": "فَلَا تَدْعُ مَعَ اللَّهِ إِلَـٰهًا آخَرَ فَتَكُونَ مِنَ الْمُعَذَّبِينَ"
      },
      {
        "ayahNumber": "214",
        "ayahText": "وَأَنذِرْ عَشِيرَتَكَ الْأَقْرَبِينَ"
      },
      {
        "ayahNumber": "215",
        "ayahText": "وَاخْفِضْ جَنَاحَكَ لِمَنِ اتَّبَعَكَ مِنَ الْمُؤْمِنِينَ"
      },
      {
        "ayahNumber": "216",
        "ayahText": "فَإِنْ عَصَوْكَ فَقُلْ إِنِّي بَرِيءٌ مِّمَّا تَعْمَلُونَ"
      },
      {
        "ayahNumber": "217",
        "ayahText": "وَتَوَكَّلْ عَلَى الْعَزِيزِ الرَّحِيمِ"
      },
      {
        "ayahNumber": "218",
        "ayahText": "الَّذِي يَرَاكَ حِينَ تَقُومُ"
      },
      {
        "ayahNumber": "219",
        "ayahText": "وَتَقَلُّبَكَ فِي السَّاجِدِينَ"
      },
      {
        "ayahNumber": "220",
        "ayahText": "إِنَّهُ هُوَ السَّمِيعُ الْعَلِيمُ"
      },
      {
        "ayahNumber": "221",
        "ayahText": "هَلْ أُنَبِّئُكُمْ عَلَىٰ مَن تَنَزَّلُ الشَّيَاطِينُ"
      },
      {
        "ayahNumber": "222",
        "ayahText": "تَنَزَّلُ عَلَىٰ كُلِّ أَفَّاكٍ أَثِيمٍ"
      },
      {
        "ayahNumber": "223",
        "ayahText": "يُلْقُونَ السَّمْعَ وَأَكْثَرُهُمْ كَاذِبُونَ"
      },
      {
        "ayahNumber": "224",
        "ayahText": "وَالشُّعَرَاءُ يَتَّبِعُهُمُ الْغَاوُونَ"
      },
      {
        "ayahNumber": "225",
        "ayahText": "أَلَمْ تَرَ أَنَّهُمْ فِي كُلِّ وَادٍ يَهِيمُونَ"
      },
      {
        "ayahNumber": "226",
        "ayahText": "وَأَنَّهُمْ يَقُولُونَ مَا لَا يَفْعَلُونَ"
      },
      {
        "ayahNumber": "227",
        "ayahText": "إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ وَذَكَرُوا اللَّهَ كَثِيرًا وَانتَصَرُوا مِن بَعْدِ مَا ظُلِمُوا ۗ وَسَيَعْلَمُ الَّذِينَ ظَلَمُوا أَيَّ مُنقَلَبٍ يَنقَلِبُونَ"
      }
    ]
  }
}