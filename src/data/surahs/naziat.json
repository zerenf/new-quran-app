{
	"naziat": {
		"surahNumber": "79",
		"surahName": "النازعات",
		"surahNameTr": "Nâziât",
		"ayats": [
			{
				"ayahNumber": "1",
				"ayahText": "وَالنَّازِعَاتِ غَرْقًا"
			},
			{
				"ayahNumber": "2",
				"ayahText": "وَالنَّاشِطَاتِ نَشْطًا"
			},
			{
				"ayahNumber": "3",
				"ayahText": "وَالسَّابِحَاتِ سَبْحًا"
			},
			{
				"ayahNumber": "4",
				"ayahText": "فَالسَّابِقَاتِ سَبْقًا"
			},
			{
				"ayahNumber": "5",
				"ayahText": "فَالْمُدَبِّرَاتِ أَمْرًا"
			},
			{
				"ayahNumber": "6",
				"ayahText": "يَوْمَ تَرْجُفُ الرَّاجِفَةُ"
			},
			{
				"ayahNumber": "7",
				"ayahText": "تَتْبَعُهَا الرَّادِفَةُ"
			},
			{
				"ayahNumber": "8",
				"ayahText": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ"
			},
			{
				"ayahNumber": "9",
				"ayahText": "أَبْصَارُهَا خَاشِعَةٌ"
			},
			{
				"ayahNumber": "10",
				"ayahText": "يَقُولُونَ أَإِنَّا لَمَرْدُودُونَ فِي الْحَافِرَةِ"
			},
			{
				"ayahNumber": "11",
				"ayahText": "أَإِذَا كُنَّا عِظَامًا نَّخِرَةً"
			},
			{
				"ayahNumber": "12",
				"ayahText": "قَالُوا تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ"
			},
			{
				"ayahNumber": "13",
				"ayahText": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ"
			},
			{
				"ayahNumber": "14",
				"ayahText": "فَإِذَا هُم بِالسَّاهِرَةِ"
			},
			{
				"ayahNumber": "15",
				"ayahText": "هَلْ أَتَاكَ حَدِيثُ مُوسَىٰ"
			},
			{
				"ayahNumber": "16",
				"ayahText": "إِذْ نَادَاهُ رَبُّهُ بِالْوَادِ الْمُقَدَّسِ طُوًى"
			},
			{
				"ayahNumber": "17",
				"ayahText": "اذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُ طَغَىٰ"
			},
			{
				"ayahNumber": "18",
				"ayahText": "فَقُلْ هَل لَّكَ إِلَىٰ أَن تَزَكَّىٰ"
			},
			{
				"ayahNumber": "19",
				"ayahText": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ"
			},
			{
				"ayahNumber": "20",
				"ayahText": "فَأَرَاهُ الْآيَةَ الْكُبْرَىٰ"
			},
			{
				"ayahNumber": "21",
				"ayahText": "فَكَذَّبَ وَعَصَىٰ"
			},
			{
				"ayahNumber": "22",
				"ayahText": "ثُمَّ أَدْبَرَ يَسْعَىٰ"
			},
			{
				"ayahNumber": "23",
				"ayahText": "فَحَشَرَ فَنَادَىٰ"
			},
			{
				"ayahNumber": "24",
				"ayahText": "فَقَالَ أَنَا رَبُّكُمُ الْأَعْلَىٰ"
			},
			{
				"ayahNumber": "25",
				"ayahText": "فَأَخَذَهُ اللَّهُ نَكَالَ الْآخِرَةِ وَالْأُولَىٰ"
			},
			{
				"ayahNumber": "26",
				"ayahText": "إِنَّ فِي ذَٰلِكَ لَعِبْرَةً لِّمَن يَخْشَىٰ"
			},
			{
				"ayahNumber": "27",
				"ayahText": "أَأَنتُمْ أَشَدُّ خَلْقًا أَمِ السَّمَاءُ ۚ بَنَاهَا"
			},
			{
				"ayahNumber": "28",
				"ayahText": "رَفَعَ سَمْكَهَا فَسَوَّاهَا"
			},
			{
				"ayahNumber": "29",
				"ayahText": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَاهَا"
			},
			{
				"ayahNumber": "30",
				"ayahText": "وَالْأَرْضَ بَعْدَ ذَٰلِكَ دَحَاهَا"
			},
			{
				"ayahNumber": "31",
				"ayahText": "أَخْرَجَ مِنْهَا مَاءَهَا وَمَرْعَاهَا"
			},
			{
				"ayahNumber": "32",
				"ayahText": "وَالْجِبَالَ أَرْسَاهَا"
			},
			{
				"ayahNumber": "33",
				"ayahText": "مَتَاعًا لَّكُمْ وَلِأَنْعَامِكُمْ"
			},
			{
				"ayahNumber": "34",
				"ayahText": "فَإِذَا جَاءَتِ الطَّامَّةُ الْكُبْرَىٰ"
			},
			{
				"ayahNumber": "35",
				"ayahText": "يَوْمَ يَتَذَكَّرُ الْإِنسَانُ مَا سَعَىٰ"
			},
			{
				"ayahNumber": "36",
				"ayahText": "وَبُرِّزَتِ الْجَحِيمُ لِمَن يَرَىٰ"
			},
			{
				"ayahNumber": "37",
				"ayahText": "فَأَمَّا مَن طَغَىٰ"
			},
			{
				"ayahNumber": "38",
				"ayahText": "وَآثَرَ الْحَيَاةَ الدُّنْيَا"
			},
			{
				"ayahNumber": "39",
				"ayahText": "فَإِنَّ الْجَحِيمَ هِيَ الْمَأْوَىٰ"
			},
			{
				"ayahNumber": "40",
				"ayahText": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِ وَنَهَى النَّفْسَ عَنِ الْهَوَىٰ"
			},
			{
				"ayahNumber": "41",
				"ayahText": "فَإِنَّ الْجَنَّةَ هِيَ الْمَأْوَىٰ"
			},
			{
				"ayahNumber": "42",
				"ayahText": "يَسْأَلُونَكَ عَنِ السَّاعَةِ أَيَّانَ مُرْسَاهَا"
			},
			{
				"ayahNumber": "43",
				"ayahText": "فِيمَ أَنتَ مِن ذِكْرَاهَا"
			},
			{
				"ayahNumber": "44",
				"ayahText": "إِلَىٰ رَبِّكَ مُنتَهَاهَا"
			},
			{
				"ayahNumber": "45",
				"ayahText": "إِنَّمَا أَنتَ مُنذِرُ مَن يَخْشَاهَا"
			},
			{
				"ayahNumber": "46",
				"ayahText": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوا إِلَّا عَشِيَّةً أَوْ ضُحَاهَا"
			}
		]
	}
}
